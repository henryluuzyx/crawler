<script>
var arr_baomoiprtaichinhnoibat = [
];;
if (arr_baomoiprtaichinhnoibat.length > 0) {
var slots_baomoiprtaichinhnoibat = new Array;
var prefix = "pr_baomoiprtaichinhnoibat_";
for (var i = 1; i <= 10; i++) {
var id = prefix + i;
var obj = document.getElementById(id);
if (obj == null) break;
else slots_baomoiprtaichinhnoibat.push(obj);
}
var total = 100.0;
var dist_baomoiprtaichinhnoibat = [];
for (var index = 0; index < slots_baomoiprtaichinhnoibat.length; index++) {
var t = Math.floor(Math.random() * total);
var n = 0;
for (var i = 0; i < arr_baomoiprtaichinhnoibat.length; i++) {
n += arr_baomoiprtaichinhnoibat[i].share;
if (n >= t) {
total -= arr_baomoiprtaichinhnoibat[i].share;
dist_baomoiprtaichinhnoibat.push(arr_baomoiprtaichinhnoibat[i].value);
arr_baomoiprtaichinhnoibat.splice(i, 1);
break;
}
}
}
for (var index = 0; index < Math.min(dist_baomoiprtaichinhnoibat.length, slots_baomoiprtaichinhnoibat.length) ; index++)
slots_baomoiprtaichinhnoibat[index].outerHTML = dist_baomoiprtaichinhnoibat[index];
}
</script>
