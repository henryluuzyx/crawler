    var article_description = 'Trong+qu%C3%BD+II%2C+%C4%91%C6%A1n+v%E1%BB%8B+n%C3%A0y+s%E1%BA%BD+gi%E1%BB%9Bi+thi%E1%BB%87u+d%E1%BB%B1+%C3%A1n+The+ZEN+Residence+-+chung+c%C6%B0+cao+c%E1%BA%A5p+th%E1%BB%A9+3+t%E1%BA%A1i+khu+%C4%91%C3%B4+th%E1%BB%8B+Gamuda+Gardens%2C+l%E1%BA%A5y+c%E1%BA%A3m+h%E1%BB%A9ng+t%E1%BB%AB+phong+c%C3%A1ch+ki%E1%BA%BFn+tr%C3%BAc+Zen.';
    var article_image = 'http://img.f26.kinhdoanh.vnecdn.net/2017/03/16/15-3-201719-426049694-2888-1489633151_490x294.jpeg';
			
    $(document).ready(function(){
        //init send mail
        sendmail.init({
            article_link: article_url,
            article_title: 'Gamuda Land mang kiến trúc Zen vào dự án chung cư cao cấp mới - VnExpress Kinh Doanh',
            cate_link: '/tin-tuc/bat-dong-san',
            cate_name: 'Bất động sản',
