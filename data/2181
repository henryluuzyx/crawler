                        <li><a href="http://video.vnexpress.net" onclick="ga('t0.send', 'event', 'Menu', 'Click', 'Video');" class="mnu_video">Video</a></li>
						<li><a class="mnu_cuoi" href="http://vnexpress.net/tin-tuc/cuoi" onclick="ga('t0.send', 'event', 'Menu', 'Click', 'Cười');">Cười</a></li>
						<li class="end"><a class="mnu_raovat" href="http://raovat.vnexpress.net" onclick="ga('t0.send', 'event', 'Menu', 'Click', 'Rao vặt');">Rao vặt</a></li>
                    </ul>
