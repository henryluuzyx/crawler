<script>
var arr_baomoiprgiaothongnoibat = [
];;
if (arr_baomoiprgiaothongnoibat.length > 0) {
var slots_baomoiprgiaothongnoibat = new Array;
var prefix = "pr_baomoiprgiaothongnoibat_";
for (var i = 1; i <= 10; i++) {
var id = prefix + i;
var obj = document.getElementById(id);
if (obj == null) break;
else slots_baomoiprgiaothongnoibat.push(obj);
}
var total = 100.0;
var dist_baomoiprgiaothongnoibat = [];
for (var index = 0; index < slots_baomoiprgiaothongnoibat.length; index++) {
var t = Math.floor(Math.random() * total);
var n = 0;
for (var i = 0; i < arr_baomoiprgiaothongnoibat.length; i++) {
n += arr_baomoiprgiaothongnoibat[i].share;
if (n >= t) {
total -= arr_baomoiprgiaothongnoibat[i].share;
dist_baomoiprgiaothongnoibat.push(arr_baomoiprgiaothongnoibat[i].value);
arr_baomoiprgiaothongnoibat.splice(i, 1);
break;
}
}
}
for (var index = 0; index < Math.min(dist_baomoiprgiaothongnoibat.length, slots_baomoiprgiaothongnoibat.length) ; index++)
slots_baomoiprgiaothongnoibat[index].outerHTML = dist_baomoiprgiaothongnoibat[index];
}
</script>
