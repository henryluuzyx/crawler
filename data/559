                var current_datetime = new Date();
                var date_picker_hidden_input = document.getElementById('datepicker_value');
                var data_picker_trigger = document.getElementById('datepicker_cate');
                var picker = new Pikaday(
                {
                    field: date_picker_hidden_input,
                    trigger: data_picker_trigger,
                    firstDay: 1,
                    i18n: {
                        previousMonth : 'Tháng trước',
                        nextMonth     : 'Tháng tới',
                        months        : ['Tháng 1','Tháng 2','Tháng 3','Tháng 4','Tháng 5','Tháng 6','Tháng 7','Tháng 8','Tháng 9','Tháng 10','Tháng 11','Tháng 12'],
