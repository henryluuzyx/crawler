		function setCookieVistorID(Name, Domain, Value)
		{
				var cstr = Name.concat('=').concat(Value);				
				cstr=cstr.concat(';domain=').concat(Domain);			
				var Expires=(new Date(2020,11,14)).toGMTString();				
				document.cookie=cstr.concat(';expires=').concat(Expires);
		}
		
		function setCookieVistorIDTime(Name, Domain, Value)
		{
				var cstr = Name.concat('=').concat(Value);				
				cstr=cstr.concat(';domain=').concat(Domain);							
				var Expires=new Date().addHours(2).toGMTString();
				document.cookie=cstr.concat(';expires=').concat(Expires);
		}
		
		function S4() {
		   return (((1+Math.random())*0x10000)|0).toString(16).substring(1);
		}
		function guid() {
		   return (S4()+S4()+"-"+S4()+"-"+S4()+"-"+S4()+"-"+S4()+S4()+S4());
		}
		function guidGenerator() {
		    var S4 = function() {
		       return (((1+Math.random())*0x10000)|0).toString(16).substring(1);
		    };
		    return (S4()+S4()+"-"+S4()+"-"+S4()+"-"+S4()+"-"+S4()+S4()+S4());
		}
		var VisitorID = getCookie('VisitorID','');
		var VisitorIDTime = getCookie('VisitorIDTime','');
		var OldVisit = 0;
		var OldVisitTime = 0;
		Date.prototype.addHours= function(h){
			this.setHours(this.getHours()+h);
			return this;
		}		
		if (VisitorID==""){
			VisitorID = guidGenerator();
			setCookieVistorID('VisitorID','vnexpress.net',VisitorID);			
		}else{
			OldVisit = 1;
		}
		if (VisitorIDTime==""){
			VisitorIDTime = guidGenerator();			
			setCookieVistorIDTime('VisitorIDTime','vnexpress.net',VisitorIDTime);
		}else{
			OldVisitTime = 1;
		}
	</script>

