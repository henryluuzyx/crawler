    <script language="javascript" type="text/lazyscript">
        var video_slide_maxtab = 5;
        var video_slide_count = 25;
        var video_slide_currenttab = 1;
        PrintInfo();
        function video_slide_forward() {
            video_slide_currenttab++;
            if (video_slide_currenttab > video_slide_maxtab) video_slide_currenttab = video_slide_maxtab;

            var leftpx = (video_slide_currenttab - 1) * (-946);
            $('#video-slide').animate({ left: leftpx }, 500);

            PrintInfo();
        }
        function video_slide_backward() {
            video_slide_currenttab--;
            if (video_slide_currenttab < 1) video_slide_currenttab = 1;
            var leftpx = (video_slide_currenttab - 1) * (-946);
            $('#video-slide').animate({ left: leftpx }, 500);

            PrintInfo();
        }
        function PrintInfo() {
            var from = video_slide_currenttab * 5 - 4;
            var to = video_slide_currenttab * 5;
            $('#video-slide-info').html(from + ' - ' + to + ' / ' + video_slide_count);
        }

        $(".sprite-dantri-new.prev").click(video_slide_backward);
        $(".sprite-dantri-new.next").click(video_slide_forward);
    </script>
    
