for (var i = 1; i <= 10; i++) {
var id = prefix + i;
var obj = document.getElementById(id);
if (obj == null) break;
else slots_baomoiprcnttvienthongnoibat.push(obj);
}
var total = 100.0;
var dist_baomoiprcnttvienthongnoibat = [];
for (var index = 0; index < slots_baomoiprcnttvienthongnoibat.length; index++) {
var t = Math.floor(Math.random() * total);
var n = 0;
for (var i = 0; i < arr_baomoiprcnttvienthongnoibat.length; i++) {
n += arr_baomoiprcnttvienthongnoibat[i].share;
if (n >= t) {
total -= arr_baomoiprcnttvienthongnoibat[i].share;
dist_baomoiprcnttvienthongnoibat.push(arr_baomoiprcnttvienthongnoibat[i].value);
arr_baomoiprcnttvienthongnoibat.splice(i, 1);
break;
}
}
}
for (var index = 0; index < Math.min(dist_baomoiprcnttvienthongnoibat.length, slots_baomoiprcnttvienthongnoibat.length) ; index++)
slots_baomoiprcnttvienthongnoibat[index].outerHTML = dist_baomoiprcnttvienthongnoibat[index];
}
