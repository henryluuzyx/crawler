$(document).ready(function(){
    $.getScript("http://st.f2.vnecdn.net/responsive/j/v16/owl.carousel.min.js", function(){
        var owl_slider_khuyenmai = $("#km_dulich #owl-demo");
        owl_slider_khuyenmai.owlCarousel({
            autoPlay: 15000,
            items : 1,
            itemsDesktop : [1199,1],
            itemsTablet: [600,1], //2 items between 600 and 0
            itemsDesktopSmall : [900,1],// betweem 900px and 601px
            itemsMobile     :[479,1],
            pagination:false,
                    });
        $("#km_dulich .next").click(function(){
            owl_slider_khuyenmai.trigger('owl.next');
        });
        $("#km_dulich .prev").click(function(){
            owl_slider_khuyenmai.trigger('owl.prev');
        });
    });
});
