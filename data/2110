	//var PAGE_FOLDER = 1;

	function tabProduct(nameTab) {
        $('.' + nameTab + ' a').click(function() {
            var $this = $(this);
            var id = $this.attr("rel");
            $('.' + nameTab).parent().removeClass('mtd-act');
            $('.' + nameTab).parent().addClass('mtd-nor');
            $('.' + id).children().removeClass('mtd-nor');
            $('.' + id).children().addClass('mtd-act');
            $('.tbl').css('display', 'none');
            $('#' + id).css('display', 'block');
			jQuery('.scroll_box').jScrollPane({ showArrows: true });// scroll rate table
        });

    }
	$(function() {
		//tabProduct('mmid');
		jQuery('.scroll_box').jScrollPane({ showArrows: true });// scroll rate table
	});
    $(document).ready(function(){
        $('#sort_bottom td a, #sort_top .sort_top_child a').each(function(key, val){
            var col = $(this).parent().attr('data-sort');
            if(col == 'education' || col == 'birthday')
            {
                $(this).append('<br /><span class="sxep '+col+'"></span>');
            }
            else
            {
                $(this).append('<span class="sxep '+col+'"></span>');
            }
        });
        var sort =  '.level_present';
                var sort_value =    'sortdow';
                $(sort).addClass(sort_value);
    });
</script>


