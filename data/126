</section>
<script>
var arr_baomoiprdienanhtruyenhinhnoibat = [
];;
if (arr_baomoiprdienanhtruyenhinhnoibat.length > 0) {
var slots_baomoiprdienanhtruyenhinhnoibat = new Array;
var prefix = "pr_baomoiprdienanhtruyenhinhnoibat_";
for (var i = 1; i <= 10; i++) {
var id = prefix + i;
var obj = document.getElementById(id);
if (obj == null) break;
else slots_baomoiprdienanhtruyenhinhnoibat.push(obj);
}
var total = 100.0;
var dist_baomoiprdienanhtruyenhinhnoibat = [];
for (var index = 0; index < slots_baomoiprdienanhtruyenhinhnoibat.length; index++) {
var t = Math.floor(Math.random() * total);
var n = 0;
for (var i = 0; i < arr_baomoiprdienanhtruyenhinhnoibat.length; i++) {
n += arr_baomoiprdienanhtruyenhinhnoibat[i].share;
if (n >= t) {
total -= arr_baomoiprdienanhtruyenhinhnoibat[i].share;
dist_baomoiprdienanhtruyenhinhnoibat.push(arr_baomoiprdienanhtruyenhinhnoibat[i].value);
arr_baomoiprdienanhtruyenhinhnoibat.splice(i, 1);
break;
}
}
}
for (var index = 0; index < Math.min(dist_baomoiprdienanhtruyenhinhnoibat.length, slots_baomoiprdienanhtruyenhinhnoibat.length) ; index++)
slots_baomoiprdienanhtruyenhinhnoibat[index].outerHTML = dist_baomoiprdienanhtruyenhinhnoibat[index];
}
</script>
