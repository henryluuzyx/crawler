<script type="text/javascript">
$('#storybox50079 #abody').css('min-height', $('#storybox50079 > article > aside').height());
var leftboxid, leftboxtop, leftboxbot, rightboxtop, rightboxbot;
var rightstickyheight = $('#rightsticky').height();
function updateleftpos(id) {
try {
if (id != $('#hdCurrentContent').val()) return;
leftboxid = 'bannerleft' + id;
if ($('#' + leftboxid + ' > .banner').children().length == 0) {
leftboxid = 'topviewleft' + id;
leftboxtop = $('#floatingtopdock' + id).position().top - 20;
} else {
leftboxtop = $('#floatingtopdockads' + id).position().top - 20;
}
var bot = leftboxbot;
leftboxbot = $('#storybox' + id).find(".tags").position().top - $('#' + leftboxid).outerHeight() - 50;
if (leftboxbot != bot && $('#' + leftboxid).css('position') == 'absolute') {
$('#' + leftboxid).css({ 'position': 'absolute', 'top': leftboxbot + 'px' });
}
}catch(e){}
rightstickyheight = $('#rightsticky').height();
rightboxtop = $('#rightstickyanchor').position().top - 40;
rightboxbot = $('footer.site-foot').position().top - rightstickyheight - 40;
}
var curcontentid = 50079;
//relateds
var relateds = [];
//$('#relatednews li a').each(function() {
// if (relateds.length >= 3) return;
// relateds.push($(this).attr('rel'));
//});
if (ismobile) $('#rightcolumn').remove();
var dones = [];
$(document).ready(function() {
//$('.fakestoryads').css('height', $('.storyads').height()).hide();
if (!ismobile) {
$('#storybox50079').css('min-height', $('#rightcolumn').height() + 'px');
window.setTimeout(function() { $('#storybox50079').css('min-height', $('#rightcolumn').height() + 'px'); }, 3000);
//$('#rightcolumn').css('position', 'relative').css('top', $('.storybox:first').offset().top - $('.main-article').offset().top).css('right', 0);
}
if (!ismobile) {
updateleftpos(curcontentid);
window.setInterval(function() { updateleftpos(curcontentid); }, 3000);
$(window).on("load", function() {updateleftpos(curcontentid);});
}
$(window).scroll(function () {
var mytop = $(window).scrollTop();
//floating left box
if (!ismobile) {
if (mytop >= leftboxtop) {
if (mytop <= leftboxbot) {
if($('#' + leftboxid).css('position') != 'fixed') {$('#' + leftboxid).css({ 'position': 'fixed', 'top': '40px' }).width('160px');}
} else {
if($('#' + leftboxid).css('position') != 'absolute') {$('#' + leftboxid).css({ 'position': 'absolute', 'top': leftboxbot + 'px' });}
}
} else {
if($('#' + leftboxid).css('position') != '') {$('#' + leftboxid).css('position', '');}
}
if (mytop >= rightboxtop) {
if (mytop <= rightboxbot - 40) {
if($('#rightsticky').css('position') != 'fixed') {$('#rightsticky').css({ 'position': 'fixed', 'top': '40px' }).width('300px');}
} else {
if($('#rightsticky').css('position') != 'absolute') {$('#rightsticky').css({ 'position': 'absolute', 'top': rightboxbot + 'px' });}
}
} else {
if($('#rightsticky').css('position') != '') {$('#rightsticky').css('position', '');}
}
}
if ($('#endtimeline').attr('rel') == '') {
var endtimeline = $('#endtimeline').offset().top - ($(window).height() * 2 / 3);
if (mytop >= endtimeline) {
loadmoretimeline();
}
}
});
});
//$('#timeline article').each(function() {
// if ($('#readmorebox article[rel="' + $(this).attr('rel') + '"]').length > 0) $(this).remove();
//});
function loadmoretimeline() {
$('#endtimeline').attr('rel', 'processing');
$.ajax({
url: '/storytimeline' + $('#endtimeline').attr('zone') + 'trang-' + $('#endtimeline').attr('page') + '.html' //'story.aspx?contentid=' + $(this).attr('content') + "&zoneid=" + $(this).attr('zone')
}).done(function(msg) {
var data = $('<div>' + msg + "</div>");
data.find('article').each(function() {
if ($(this).attr('rel') == curcontentid || $('#readmorebox article[rel="' + $(this).attr('rel') + '"]').length > 0 || $('#timeline article[rel="' + $(this).attr('rel') + '"]').length > 0) {
$(this).remove();
}
});
$('#timeline').append(data.html());
$('#endtimeline').attr('page', parseInt($('#endtimeline').attr('page')) + 1).attr('rel', '');
var mytop = $(window).scrollTop();
var endtimeline = $('#endtimeline').offset().top - ($(window).height() / 2);
if (mytop >= endtimeline) {
loadmoretimeline();
}
rightboxbot = $('footer.site-foot').position().top - rightstickyheight - 40;
$('#rightsticky').css({ 'position': 'fixed', 'top': '40px' }).width('300px');
});
}
</script>
