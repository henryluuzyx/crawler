<script language="javascript" type="text/javascript">
var tab_active  = 'list01';
$(document).ready(function(){
    //Init tvschedule
    TVSchedule.init();

    var intervalTV  = setInterval(function(){
        if ( typeof($.fn.DatePicker)!='undefined' || TVSchedule.timeout>=TVSchedule.max_timeout ) {
            clearInterval(intervalTV);
            TVSchedule.timeout      = 0;
            TVSchedule.include_lib  = typeof($.fn.DatePicker)=='undefined'?true:false;
            TVSchedule.tvschedule_parser_id     = 'tvschedule';
            TVSchedule.tvschedule_parser_url    = 'http://st.f2.vnecdn.net/responsive/j/v16/utils/calendar';
            TVSchedule.tvschedule_date          = '18-03-2017';
            TVSchedule.parse();
        } else {
            TVSchedule.timeout++;
        }
    }, 300);
});
