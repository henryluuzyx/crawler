                LikeParser.URL = "http://st.f2.vnecdn.net/responsive/j/v16";
                LikeParser.parse();
            }
            common.initCommon();
            /**
            * Init switch to desktop
            */
            common.switchToDesktop();

        });
        $(function() {
            if (typeof (crawler) != "undefined")
            {
                crawler.run();
            }
        });

        /*Add script menu cuoi*/
    	$('#menu_web').find('.item_have_more_menu').click(
    		function(){
    			if($(this).find('.sub_main_menu_item').css('display') == "none"){
    				$(this).find('.sub_main_menu_item').show();
    				$(this).addClass('active');
    			}
    			else{
    				$(this).find('.sub_main_menu_item').hide();
    				$(this).removeClass('active');
    			}
    		}
    	);
        </script>

