if (arr_baomoiprbongdanoibat.length > 0) {
var slots_baomoiprbongdanoibat = new Array;
var prefix = "pr_baomoiprbongdanoibat_";
for (var i = 1; i <= 10; i++) {
var id = prefix + i;
var obj = document.getElementById(id);
if (obj == null) break;
else slots_baomoiprbongdanoibat.push(obj);
}
var total = 100.0;
var dist_baomoiprbongdanoibat = [];
for (var index = 0; index < slots_baomoiprbongdanoibat.length; index++) {
var t = Math.floor(Math.random() * total);
var n = 0;
for (var i = 0; i < arr_baomoiprbongdanoibat.length; i++) {
n += arr_baomoiprbongdanoibat[i].share;
if (n >= t) {
total -= arr_baomoiprbongdanoibat[i].share;
dist_baomoiprbongdanoibat.push(arr_baomoiprbongdanoibat[i].value);
arr_baomoiprbongdanoibat.splice(i, 1);
break;
}
}
}
for (var index = 0; index < Math.min(dist_baomoiprbongdanoibat.length, slots_baomoiprbongdanoibat.length) ; index++)
slots_baomoiprbongdanoibat[index].outerHTML = dist_baomoiprbongdanoibat[index];
}
