<script>
//tab danh gia chi tiet
$('.tab_danhgia_header a').click(function(){
	var curenttab= $(this).attr('rel');
	$('.tab_danhgia_header a.active').removeClass('active');
	$(this).addClass('active');
	$('.content_danhgia_chitiet').hide();
	$('#'+curenttab).show();	
});
function showprevcompare(){
    var page = parseInt($("#comparenext").attr("data-page"));  
    var flag = parseInt($("#comparenext").attr("data-flag"));
    var manu = parseInt($("#comparefiltermanu").val());
    var price = $("#comparefilterprice").val();
    $.ajax({
        type: "GET",
        dataType : "JSON",
        url: "/detail/ajaxreviewcompare",
        data: { type: 'prev', page: (page - 1), id: 3533013 , flag: flag, manu: manu, price: price }
    })
    .done(function( data ) {
        if (data['error'] == 1){
            $("#comparearea").html(data.revision);
        }
        else{
            $("#comparearea").html(data.html);
            $("#comparenext,#compareprev").attr({"data-page": page - 1});
            /// show prev
            $("#comparenext").show();
            (page <= 2) ? $("#compareprev").hide() : $("#compareprev").show(); 
            // kiem tra viec show input
            checkcompareinput();
        }
    });
}
function shownextcompare(){
    var page = parseInt($("#comparenext").attr("data-page"));  
    var flag = parseInt($("#comparenext").attr("data-flag"));
    var manu = parseInt($("#comparefiltermanu").val());
    var price = $("#comparefilterprice").val();
    $.ajax({
        type: "GET",
        dataType : "JSON",
        url: "/detail/ajaxreviewcompare",
        data: { type: 'next', page: (page + 1), id: 3533013 , flag: flag, manu: manu, price: price}
    })
    .done(function( data ) {
        if (data['error'] == 1){
            $("#comparearea").html(data.revision);
        }
        else{
            $("#comparearea").html(data.html);
            $("#comparenext,#compareprev").attr({"data-page": page + 1, "data-flag" : data.flag})
            if (flag == 0 && data.flag > 1){
                $("#comparenext,#compareprev").attr("data-flag",data.flag);
            }
            /// show prev
            $("#compareprev").show();
            (data.shownext != 1) ? $("#comparenext").hide() : $("#comparenext").show(); 
            // kiem tra viec show input
            checkcompareinput();
        }
    });
}
function showprevcompare1(){
    var page = parseInt($("#compareprev1").attr("data-page"));  
    var flag = parseInt($("#compareprev1").attr("data-flag"));
    var manu = parseInt($("#comparefiltermanu1").val());
    var price = $("#comparefilterprice1").val();
    $.ajax({
        type: "GET",
        dataType : "JSON",
        url: "/detail/ajaxreviewcompare1",
        data: { type: 'prev', page: (page - 1), id: 3533013 , flag: flag, manu: manu, price: price,cateid:1002652 }
    })
    .done(function( data ) {
        if (data['error'] == 1){
            $("#box300_sosanhsp").html(data.revision);
        }
        else{
            $("#box300_sosanhsp").html(data.html);
            // kiem tra viec show input
            checkcompareinput();
        }
    });
}
function shownextcompare1(){
    var page = parseInt($("#comparenext1").attr("data-page"));  
    var flag = parseInt($("#comparenext1").attr("data-flag"));
    var manu = parseInt($("#comparefiltermanu1").val());
    var price = $("#comparefilterprice1").val();
    $.ajax({
        type: "GET",
        dataType : "JSON",
        url: "/detail/ajaxreviewcompare1",
        data: { type: 'next', page: (page + 1), id: 3533013 , flag: flag, manu: manu, price: price,cateid:1002652}
    })
    .done(function( data ) {
        if (data['error'] == 1){
            $("#box300_sosanhsp").html(data.revision);
        }
        else{
            $("#box300_sosanhsp").html(data.html);
            // kiem tra viec show input
            checkcompareinput();
        }
    });
}
var arrCompare = new Array();
// kiem tra nhung input nao da duoc tick
function checkcompareinput(){
    $(".compareinputadd").each(function(index){
        var value = parseInt($(this).val());
        if ($.inArray(value, arrCompare) > -1){
            $(this).attr('checked',true);
        }
    });
}
// them id vao mang khi click checkbox so sanh
function compareadd(id){
    // neu id da co trong mang
    if ($.inArray(id, arrCompare) > -1){
        var index = arrCompare.indexOf(id);
        if (index > -1) {
            arrCompare.splice(index, 1);
        }
        return true;
    }
    /// push them vao mang
    var length = arrCompare.length;
    if (length >=  3){
        alert("Bạn đã thêm quá số lượng sản phẩm so sánh");
        $(".compareinputid_"+id).attr('checked',false);
    }
    else {
        arrCompare.push(id);
    }
    return true;
}
/////////////// SUBMIT FORM SO SANH TRUYEN SANG TRANG SO SANH ///////////
function comparesubmit(){
    if (arrCompare.length <= 0){
        alert("Bạn phải chọn ít nhất 1 sản phẩm để so sánh");
        return false;
    }
    var url = '/detail/compare?arrid[]=3533013';
    $.each(arrCompare, function( index, value ) {
        url += '&arrid[]=' + value;
    });
    window.location.href= url;
}
//////////// CHON DROPBOX ///////////////////////////
function comparefilteroption(){
    var manu = parseInt($("#comparefiltermanu").val());
    var price = $("#comparefilterprice").val();
    $.ajax({
        type: "GET",
        dataType : "JSON",
        url: "/detail/ajaxreviewcompare",
        data: { type: 'show', page: 1, id: 3533013, manu: manu, price: price}
    })
    .done(function( data ) {
        if (data['error'] == 1){
            $("#comparearea").html(data.revision);
        }
        else{
            $("#comparearea").html(data.html);
            (data.shownext != 1) ? $("#comparenext").hide() : $("#comparenext").show(); 
            // kiem tra viec show input
            checkcompareinput();
        }
        $("#searchcompare").val("");
    });
}
function comparefilteroption1(){
    var manu = parseInt($("#comparefiltermanu1").val());
    var price = $("#comparefilterprice1").val();
    $.ajax({
        type: "GET",
        dataType : "JSON",
        url: "/detail/ajaxreviewcompare1",
        data: { type: 'show', page: 1, id: 3533013, manu: manu, price: price,cateid:1002652}
    })
    .done(function( data ) {
        if (data['error'] == 1){
            $("#box300_sosanhsp").html(data.revision);
        }
        else{
            $("#box300_sosanhsp").html(data.html);
            // kiem tra viec show input
            checkcompareinput();
        }
    });
}
//////////// TIM KIEM SP SO SANH /////////////
$(document).ready(function() {
    var cache = {};
    $( "#searchcompare" ).autocomplete({
        minLength: 3,
        source: function( request, response ) {
            var term = request.term;
                if ( term in cache ) {
                response( cache[ term ] );
                return;
            }
            request.cateid = '1002652';
            $.getJSON( "/detail/ajaxreviewcompare", request, function( data, status, xhr ) {
                cache[ term ] = data;
                response( data );
            });
        },
    		select: function( event, ui ) {
    			var html = '<li>'
    							+'<div class="thumb">'
    							   +'<a href="'+ui.item.thumb+'" class="aImg">'
    							   +'<img alt="'+ui.item.label+'" src="'+ui.item.thumb+'">'
    							   +'</a>'
    							   +'<input autocomplete="off" class="check_in compareinputadd compareinputid_'+ui.item.id+'" onclick="compareadd('+ui.item.id+')" type="checkbox" value="'+ui.item.id+'">'
    							+'</div>'
    							+'<p><a href="' + ui.item.share_url + '">'+ui.item.label+'</a></p>'
    						+'</li>';
    			$('.list_sosanh').html(html);
    			checkcompareinput();
    			$('.prev_btt, .next_btt').hide();
          /////////////// RESET SEARCH BOX ////////////
          $('#comparefilterprice, #comparefiltermanu').find('option:first').attr('selected', 'selected');
    		}
    });

  //paser vào box 300
  $.ajax({
        type: "GET",
        dataType : "JSON",
        url: "/detail/ajaxreviewcompare1",
        data: { type: 'show', page: 1, id: 3533013, manu: 0, price: '',cateid:1002652}
    })
    .done(function( data ) {
        if (data['error'] == 1){
            $("#box300_sosanhsp").html(data.revision);
        }
        else{
            $("#box300_sosanhsp").html(data.html);
            // kiem tra viec show input
            checkcompareinput();
        }
    });
});
