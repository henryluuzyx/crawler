<script>
    function showfullcomment(id){
        $(".icon_tru_" + id).show();
        $(".icon_cong_" + id).hide();
        $(".expand_all_" + id).fadeIn(500);
        $(".ext_text_" + id).hide();
        
    }
    function showshortcomment(id){
        $(".icon_tru_" + id).hide();
        $(".icon_cong_" + id).show();
        $(".expand_all_" + id).hide();
        $(".ext_text_" + id).show();
    }
</script><div class="box_category width_common hidden320">
