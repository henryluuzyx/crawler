function showxemayData(data, brand, brand_value_1, brand_index)
{
    var html = '';
	var price_value = 'all';
    var price_value_arr = [];
    if (price_value.indexOf('?') > -1) {
        var price_value_split = price_value.split('?');
        price_value = price_value_split[0];
    }
    if(typeof(price_value) != 'undefined' && price_value != 'all')
    {
        price_value_arr = price_value.split('-');
        price_value_arr[0] = parseInt(price_value_arr[0].trim());
        price_value_arr[1] = parseInt(price_value_arr[1].trim());
    }
    $.each(data, function(k, v){
        if(typeof(v.carName) != 'undefined')
        {
            var carPrice = parseInt(v.carPrice.replace('.', '').trim());
            if(
                (brand_value_1 == 'all' && price_value == 'all') ||
                (brand_value_1 != 'all' && price_value == 'all' && v.carBrand.trim() == brand_value_1.trim()) ||
                (brand_value_1 == 'all' && price_value != 'all' && carPrice >= price_value_arr[0] && carPrice <= price_value_arr[1]) ||
                (brand_value_1 != 'all' && price_value != 'all' && carPrice >= price_value_arr[0] && carPrice <= price_value_arr[1] && v.carBrand.trim() == brand_value_1.trim())
            )
            {
				if(brand_index == 1)
					$('#model_filter_xe_may_1').append('<option value="' + v.carId + '">' + v.carName + '</option>');
				else if(brand_index == 2)
					$('#model_filter_xe_may_2').append('<option value="' + v.carId + '">' + v.carName + '</option>');
            }
        }
    });
    return true;
}
function brand_filter1_xe_may_change(type)
{
	 var brand_value_1 = $('#brand_filter_xe_may_1').val();
	 var brand_value_2 = $('#brand_filter_xe_may_2').val();
	 if(type == 1)
	 {
		 document.getElementById("model_filter_xe_may_1").options.length=0
		 showxemayData(data, brand, brand_value_1, 1);
	 }
	 else
	 {
		document.getElementById("model_filter_xe_may_2").options.length=0
		showxemayData(data, brand, brand_value_2, 2);
	 }
}
function comparexemayFunction()
{
	var model_filter1 = $('#model_filter_xe_may_1').val();
	var model_filter2 = $('#model_filter_xe_may_2').val();
	if(model_filter1 == 'all' || model_filter2 == 'all')
	{
		alert('Bạn cần chọn mẫu xe cần so sánh');
	}
	else if(model_filter1 == model_filter2)
	{
		alert('Không so sánh hai mẫu xe giống nhau');
	}
	else
	{
		window.location.assign("/interactive/2016/bang-gia-xe-may/so-sanh-xe.html?m1="+model_filter1+"&m2=" + model_filter2);
	}
}
$(document).ready(function(){
	$('.xe_may_submit').bind('click', function(){
		var brand = $('#brand_filter_xe_may').val();
		var price = $('#price_filter_xe_may').val();
		var url = '/interactive/2016/bang-gia-xe-may/#'+brand+';'+price;
		window.location.href = url;
	});
	 // get state
});

</script>
