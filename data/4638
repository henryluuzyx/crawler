    $.each(jsonStr, function(index, value) {
		var randomItem = [value.title, value.lead, value.share_url, value.thumbnail_url, value.icons, value.comment];
		arrData.push(randomItem);
    });
    arrData = shuffle(arrData);  
    var html;
    html = '\
    <div class="width_common box_num02">\
      <div class="thumb_relative">\
      	<a href="'+arrData[0][2]+'"><img alt="" src="'+arrData[0][3]+'" /></a>\
       </div>\
      <div class="title_news"><a class="txt_link" href="'+arrData[0][2]+'">'+arrData[0][0]+' <span class="no_wrap">'+arrData[0][4]+'</span></a> '+arrData[0][5]+'</div>\
      <div class="news_lead" data-mobile-href="'+arrData[0][2]+'">'+arrData[0][1]+'</div>\
    </div>\
    <ul class="list_new_140 list_col">\
        <li>\
            <div class="thumb_140 width_common">\
            <a href="'+arrData[1][2]+'"><img src="'+arrData[1][3]+'" alt=""></a>\
            </div>\
