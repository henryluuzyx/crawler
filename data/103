];;
if (arr_baomoiprdaotaothicunoibat.length > 0) {
var slots_baomoiprdaotaothicunoibat = new Array;
var prefix = "pr_baomoiprdaotaothicunoibat_";
for (var i = 1; i <= 10; i++) {
var id = prefix + i;
var obj = document.getElementById(id);
if (obj == null) break;
else slots_baomoiprdaotaothicunoibat.push(obj);
}
var total = 100.0;
var dist_baomoiprdaotaothicunoibat = [];
for (var index = 0; index < slots_baomoiprdaotaothicunoibat.length; index++) {
var t = Math.floor(Math.random() * total);
var n = 0;
for (var i = 0; i < arr_baomoiprdaotaothicunoibat.length; i++) {
n += arr_baomoiprdaotaothicunoibat[i].share;
if (n >= t) {
total -= arr_baomoiprdaotaothicunoibat[i].share;
dist_baomoiprdaotaothicunoibat.push(arr_baomoiprdaotaothicunoibat[i].value);
arr_baomoiprdaotaothicunoibat.splice(i, 1);
break;
}
}
}
for (var index = 0; index < Math.min(dist_baomoiprdaotaothicunoibat.length, slots_baomoiprdaotaothicunoibat.length) ; index++)
slots_baomoiprdaotaothicunoibat[index].outerHTML = dist_baomoiprdaotaothicunoibat[index];
}
