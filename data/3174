var total_page = 9;
var total_item = 8;
// parser data
$(document).ready(function(){
     loadDataGocNhin(0);
     $('.pagination_news a, #to_be_continue_ctg a').bind('click', function(){
        var page = $(this).attr('data-page');
        page = parseInt(page);
        if(page >= 0)
        {
            loadDataGocNhin(page);
        }
     });
});

function htmlspecialchars_decode(b,a){var e=0,c=0,f=!1;"undefined"===typeof a&&(a=2);b=b.toString().replace(/&lt;/g,"<").replace(/&gt;/g,">");var d={ENT_NOQUOTES:0,ENT_HTML_QUOTE_SINGLE:1,ENT_HTML_QUOTE_DOUBLE:2,ENT_COMPAT:2,ENT_QUOTES:3,ENT_IGNORE:4};0===a&&(f=!0);if("number"!==typeof a){a=[].concat(a);for(c=0;c<a.length;c++)0===d[a[c]]?f=!0:d[a[c]]&&(e|=d[a[c]]);a=e}a&d.ENT_HTML_QUOTE_SINGLE&&(b=b.replace(/&#0*39;/g,"'"));f||(b=b.replace(/&quot;/g,'"'));return b=b.replace(/&amp;/g,"&")};
function unserialize(p){var q=this;error=function(e,f,h,a){throw new q.window[e](f,h,a);};read_until=function(e,f,h){for(var a=2,b=[],g=e.slice(f,f+1);g!=h;)a+f>e.length&&error("Error","Invalid"),b.push(g),g=e.slice(f+(a-1),f+a),a+=1;return[b.length,b.join("")]};read_chrs=function(e,f,h){var a,b,g;g=[];for(a=0;a<h;a++)b=e.slice(f+(a-1),f+a),g.push(b),b=b.charCodeAt(0),b=128>b?0:2048>b?1:2,h-=b;return[g.length,g.join("")]};_unserialize=function(e,f){var h,a,b,g,d,c,k,m,l;d=0;var n=function(a){return a};
f||(f=0);h=e.slice(f,f+1).toLowerCase();a=f+2;switch(h){case "i":n=function(a){return parseInt(a,10)};c=read_until(e,a,";");d=c[0];c=c[1];a+=d+1;break;case "b":n=function(a){return 0!==parseInt(a,10)};c=read_until(e,a,";");d=c[0];c=c[1];a+=d+1;break;case "d":n=function(a){return parseFloat(a)};c=read_until(e,a,";");d=c[0];c=c[1];a+=d+1;break;case "n":c=null;break;case "s":c=read_until(e,a,":");d=c[0];b=c[1];a+=d+2;c=read_chrs(e,a+1,parseInt(b,10));d=c[0];c=c[1];a+=d+2;d!=parseInt(b,10)&&d!=c.length&&
error("SyntaxError","String length mismatch");break;case "a":c={};b=read_until(e,a,":");d=b[0];b=b[1];a+=d+2;d=parseInt(b,10);g=!0;for(b=0;b<d;b++)k=_unserialize(e,a),m=k[1],k=k[2],a+=m,l=_unserialize(e,a),m=l[1],l=l[2],a+=m,k!==b&&(g=!1),c[k]=l;if(g){g=Array(d);for(b=0;b<d;b++)g[b]=c[b];c=g}a+=1;break;default:error("SyntaxError","Unknown / Unhandled data type(s): "+h)}return[h,a-f,n(c)]};return _unserialize(p+"",0)[2]};

function loadDataGocNhin(page)
{
    if(page >= total_page && device_env == 1)
    {
        $('#to_be_continue_ctg').remove();
        return false;
    }
    page = parseInt(page);
    var next = page+1;
    var prev = page-1;
    var result = data[page];
    var html = '';
    $.each(result, function(key, value ) {
        var obj = unserialize(value);
        html += '<li>';
        if(typeof(obj.thumbnail_url) != 'undefined')
        {
            html += '<div class="thumb">';
            html += '<a href="'+obj.share_url+'"><img src="'+obj.thumbnail_url+'" alt="'+obj.author_name+'"></a>';
            html += '</div>';
        }
        html +=     '<div class="title">';
        html +=     '<a href="'+obj.share_url+'">'+obj.author_name+'</a>';
        html +=     '</div>';
        if(typeof(obj.job) != 'undefined' && obj.job != null)
        {
            html += '<div class="sobaiviet" style="font-size:11px;">'
            html += htmlspecialchars_decode(obj.job);
            if(typeof(obj.company) != 'undefined' && obj.company != null && obj.company != '')
            {
                html += ', <i>'+htmlspecialchars_decode(obj.company)+'</i>';
            }
            html += '</div>';
        }
        html += '</li>';
    });
    if(device_env != 1)
    {
        $('#tacgia_output').html(html);
    }
    else
    {
        $('#tacgia_output').append(html);
    }
    // lazyload
    $('img.lazy').lazyload({
		effect: 'fadeIn',
		load: function() {
			$('img.lazy').each(function() {
				if ($(this).attr('src') == $(this).attr('data-original')) {
					$(this).removeClass('lazy');
				}
			});
		}
	});
    // paging for web
    $('.pagination_news .pa_prev').attr('data-page', prev);
    $('.pagination_news .pa_next').attr('data-page', next);
    // hide page
    if(page == 0)
    {
        $('.pagination_news .pa_prev:last').hide();
    }
    else
    {
        $('.pagination_news .pa_prev:last').show();
    }
    if(page >= total_item)
    {
        $('.pagination_news .pa_next:last').hide();
    }
    else
    {
       $('.pagination_news .pa_next:last').show();
    }
    // mobile

    if(page < total_page)
    {
        $('#to_be_continue_ctg a').attr('data-page', next);
    }
}
</script>
