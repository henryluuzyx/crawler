var arr_baomoiprxeconoibat = [
];;
if (arr_baomoiprxeconoibat.length > 0) {
var slots_baomoiprxeconoibat = new Array;
var prefix = "pr_baomoiprxeconoibat_";
for (var i = 1; i <= 10; i++) {
var id = prefix + i;
var obj = document.getElementById(id);
if (obj == null) break;
else slots_baomoiprxeconoibat.push(obj);
}
var total = 100.0;
var dist_baomoiprxeconoibat = [];
for (var index = 0; index < slots_baomoiprxeconoibat.length; index++) {
var t = Math.floor(Math.random() * total);
var n = 0;
for (var i = 0; i < arr_baomoiprxeconoibat.length; i++) {
n += arr_baomoiprxeconoibat[i].share;
if (n >= t) {
total -= arr_baomoiprxeconoibat[i].share;
dist_baomoiprxeconoibat.push(arr_baomoiprxeconoibat[i].value);
arr_baomoiprxeconoibat.splice(i, 1);
break;
}
}
}
for (var index = 0; index < Math.min(dist_baomoiprxeconoibat.length, slots_baomoiprxeconoibat.length) ; index++)
slots_baomoiprxeconoibat[index].outerHTML = dist_baomoiprxeconoibat[index];
}
