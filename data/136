var arr_baomoiprkhoahoccongnghenoibat = [
];;
if (arr_baomoiprkhoahoccongnghenoibat.length > 0) {
var slots_baomoiprkhoahoccongnghenoibat = new Array;
var prefix = "pr_baomoiprkhoahoccongnghenoibat_";
for (var i = 1; i <= 10; i++) {
var id = prefix + i;
var obj = document.getElementById(id);
if (obj == null) break;
else slots_baomoiprkhoahoccongnghenoibat.push(obj);
}
var total = 100.0;
var dist_baomoiprkhoahoccongnghenoibat = [];
for (var index = 0; index < slots_baomoiprkhoahoccongnghenoibat.length; index++) {
var t = Math.floor(Math.random() * total);
var n = 0;
for (var i = 0; i < arr_baomoiprkhoahoccongnghenoibat.length; i++) {
n += arr_baomoiprkhoahoccongnghenoibat[i].share;
if (n >= t) {
total -= arr_baomoiprkhoahoccongnghenoibat[i].share;
dist_baomoiprkhoahoccongnghenoibat.push(arr_baomoiprkhoahoccongnghenoibat[i].value);
arr_baomoiprkhoahoccongnghenoibat.splice(i, 1);
break;
}
}
}
for (var index = 0; index < Math.min(dist_baomoiprkhoahoccongnghenoibat.length, slots_baomoiprkhoahoccongnghenoibat.length) ; index++)
slots_baomoiprkhoahoccongnghenoibat[index].outerHTML = dist_baomoiprkhoahoccongnghenoibat[index];
}
</script>
