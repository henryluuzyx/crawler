<script>
var arr_baomoiprkinhtenoibat = [
];;
if (arr_baomoiprkinhtenoibat.length > 0) {
var slots_baomoiprkinhtenoibat = new Array;
var prefix = "pr_baomoiprkinhtenoibat_";
for (var i = 1; i <= 10; i++) {
var id = prefix + i;
var obj = document.getElementById(id);
if (obj == null) break;
else slots_baomoiprkinhtenoibat.push(obj);
}
var total = 100.0;
var dist_baomoiprkinhtenoibat = [];
for (var index = 0; index < slots_baomoiprkinhtenoibat.length; index++) {
var t = Math.floor(Math.random() * total);
var n = 0;
for (var i = 0; i < arr_baomoiprkinhtenoibat.length; i++) {
n += arr_baomoiprkinhtenoibat[i].share;
if (n >= t) {
total -= arr_baomoiprkinhtenoibat[i].share;
dist_baomoiprkinhtenoibat.push(arr_baomoiprkinhtenoibat[i].value);
arr_baomoiprkinhtenoibat.splice(i, 1);
break;
}
}
}
for (var index = 0; index < Math.min(dist_baomoiprkinhtenoibat.length, slots_baomoiprkinhtenoibat.length) ; index++)
slots_baomoiprkinhtenoibat[index].outerHTML = dist_baomoiprkinhtenoibat[index];
}
</script>
