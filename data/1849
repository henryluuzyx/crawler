<script type="text/javascript">
var epi_width = '250px';
var epi_height = '300px';
var epi_target = 0;
var epi_border = 0;
var epi_theme = 'widgetDisplayThemeBlue';
function updateCode() {
var codeString = '';
var widgetWidth = $('#txtWidgetWidth').val();
var widgetHeight = $('#txtWidgetHeight').val();
if (/^\s*\d+\s*(px)?\s*$/.test(widgetWidth)) {
var widthNumber = /^\s*(\d+)/.exec(widgetWidth)[1];
if (widthNumber < 150) {
widgetWidth = '150px';
$('#txtWidgetWidth').value = widgetWidth;
$('#errorWidgetWidth').css("color", "#f00");
$('#errorWidgetWidth').css("fontWeight", "bold");
}
else {
$('#errorWidgetWidth').css("color", "#999");
$('#errorWidgetWidth').css("fontWeight", "normal");
}
}
if (widgetWidth !== '150px') codeString = codeString + 'epi_width = \'' + widgetWidth + '\';\r\n';
if (widgetHeight !== '300px') codeString = codeString + 'epi_height = \'' + widgetHeight + '\';\r\n';
if ($('#widgetThemeContainer').className !== 'widgetDisplayThemeBlue') codeString = codeString + 'epi_theme = \'' + $('#widgetThemeContainer').attr('class') + '\';\r\n';
if ($('#ddlWidgetBorder').val() !== 0) codeString = codeString + 'epi_border = ' + $('#ddlWidgetBorder').val() + ';\r\n';
if ($('#ddlOpenType').val() !== 0) codeString = codeString + 'epi_target = ' + $('#ddlOpenType').val() + ';\r\n';
if (codeString != '') {
codeString = '&lt;script type="text/javascript"&gt;\r\nepi_id = \'epi-widget-container\';\r\n' + codeString + '&lt;/script&gt;\r\n';
}
else {
codeString = '&lt;script type="text/javascript"&gt;\r\nepi_id = \'epi-widget-container\';\r\n&lt;/script&gt;\r\n';
}
codeString = codeString + '&lt;script type="text/javascript" src="http://www.baomoi.com/Widget/JS/Widget.js"&gt;&lt;/script&gt;\r\n';
var catId = document.getElementById('main_ddlCategory').value;
var numOfNews = document.getElementById('main_ddlNumOfNews').value;
var newsType = document.getElementById('main_ddlNewsType').value;
var displayType = document.getElementById('main_ddlDisplayType').value;
var openType = $('#ddlOpenType').val();
codeString = codeString + '&lt;script type="text/javascript" src="http://www.baomoi.com/Widget/GenerateJS.aspx?';
codeString = codeString + "zone=" + catId;
codeString = codeString + '&count=' + numOfNews + '&ntype=' + newsType + '&dtype=' + displayType + '&target=' + openType + '"&gt;&lt;/script&gt;\r\n';
$('#widgetGeneratedCode').html(codeString);
}
function getElementsByClassName(classname) {
if (document.getElementsByTagName) {
var els = document.getElementsByTagName("*");
var c = new RegExp('/b^|' + classname + '|$/b');
elem = new Array();
var n = 0;
for (var i = 0; i < els.length; i++) {
if (els[i].className) {
if (c.test(els[i].className)) {
elem[n] = els[i]; n++;
}
}
}
return elem;
}
else return false;
}
function changeTheme(element, themeName) {
var themeContainer = document.getElementById('widgetThemeContainer');
themeContainer.className = themeName;
var currentSelected = getElementsByClassName('widgetThemeSelected');
currentSelected[0].className = '';
element.className = 'widgetThemeSelected';
updateCode();
return false;
}
function changeBorder(borderBit) {
var themeContainer = document.getElementById('widgetThemeContainer');
themeContainer.removeAttribute('style');
if (borderBit == 1) {
themeContainer.style.border = 'none';
}
updateCode();
return false;
}
function changeNewsDisplay() {
var catId = document.getElementById('main_ddlCategory').value;
var objNewsType = document.getElementById('main_ddlNewsType');
var newsType = objNewsType.value;
var numOfNews = document.getElementById('main_ddlNumOfNews').value;
var displayType = document.getElementById('main_ddlDisplayType').value;
updateWidgetCallback(catId, numOfNews, newsType, displayType);
updateCode();
}
function updateWidgetCallback(catId, numOfNews, newsType, displayType) {
document.getElementById('updateProgress').style.display = "";
$('#widgetContent').css("display", "none");
var url = 'generatejs.aspx?zoneid=' + catId + "&top=" + numOfNews + "&ntype=" + newsType + "&dtype=" + displayType;
$.get(url, function (data) {
data = data.replace("outputWidget('", "").replace("</div>');", "</div>");
updateWidgetResult(data);
});
}
function updateWidgetResult(value) {
$('#widgetContent').html(value);
$('#widgetContent').css("display", "inherit");
document.getElementById('updateProgress').style.display = "none";
}
updateCode();
</script>
