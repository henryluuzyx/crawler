<script>
var arr_baomoiprthoisunoibat = [
];;
if (arr_baomoiprthoisunoibat.length > 0) {
var slots_baomoiprthoisunoibat = new Array;
var prefix = "pr_baomoiprthoisunoibat_";
for (var i = 1; i <= 10; i++) {
var id = prefix + i;
var obj = document.getElementById(id);
if (obj == null) break;
else slots_baomoiprthoisunoibat.push(obj);
}
var total = 100.0;
var dist_baomoiprthoisunoibat = [];
for (var index = 0; index < slots_baomoiprthoisunoibat.length; index++) {
var t = Math.floor(Math.random() * total);
var n = 0;
for (var i = 0; i < arr_baomoiprthoisunoibat.length; i++) {
n += arr_baomoiprthoisunoibat[i].share;
if (n >= t) {
total -= arr_baomoiprthoisunoibat[i].share;
dist_baomoiprthoisunoibat.push(arr_baomoiprthoisunoibat[i].value);
arr_baomoiprthoisunoibat.splice(i, 1);
break;
}
}
}
for (var index = 0; index < Math.min(dist_baomoiprthoisunoibat.length, slots_baomoiprthoisunoibat.length) ; index++)
slots_baomoiprthoisunoibat[index].outerHTML = dist_baomoiprthoisunoibat[index];
}
</script>
