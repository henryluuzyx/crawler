<script type="text/javascript">
/*<![CDATA[*/
	
    /*$(window).load(function(){
        $('.sort-desc').click();        
      });*/


					function replaceItem(obj){

						var a_thumbs = $(obj).find('.feature-1 a.thumbs');
						var a_title = $(obj).find('.feature-1 a.canbiet-title');
						if(a_thumbs){
							var li = '<li><a href="'+$(a_thumbs).attr('href')+'" title="'+$(a_thumbs).attr('title')+'" thumbmail="'+$(a_thumbs).find('img').attr('src')+'">'+$(a_title).html()+'</a></li>'; 
						}
						
						// li first
						var li_replace = $(obj).find('ul li:first-child a');
						var href = $(li_replace).attr('href');
						var title = $(li_replace).attr('title');
						var src = $(li_replace).attr('thumbmail');

						$(a_thumbs).attr('href',href).attr('title',title);
						var img = $(a_thumbs).find('img');
						$(img).attr('src',src).attr('title',title);
						$(a_title).attr('href',href).attr('title',title).text(title);

						$(li_replace).parent().remove();
						$(obj).find('ul').append(li);

					}

					setInterval(function(){
						var l = $('#canbiet_main_left');
						var r = $('#canbiet_main_right');
						replaceItem(l);
						replaceItem(r);
					},30000);
				
/*]]>*/
</script>
