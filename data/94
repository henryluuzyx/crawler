var arr_baomoiprchungkhoannoibat = [
];;
if (arr_baomoiprchungkhoannoibat.length > 0) {
var slots_baomoiprchungkhoannoibat = new Array;
var prefix = "pr_baomoiprchungkhoannoibat_";
for (var i = 1; i <= 10; i++) {
var id = prefix + i;
var obj = document.getElementById(id);
if (obj == null) break;
else slots_baomoiprchungkhoannoibat.push(obj);
}
var total = 100.0;
var dist_baomoiprchungkhoannoibat = [];
for (var index = 0; index < slots_baomoiprchungkhoannoibat.length; index++) {
var t = Math.floor(Math.random() * total);
var n = 0;
for (var i = 0; i < arr_baomoiprchungkhoannoibat.length; i++) {
n += arr_baomoiprchungkhoannoibat[i].share;
if (n >= t) {
total -= arr_baomoiprchungkhoannoibat[i].share;
dist_baomoiprchungkhoannoibat.push(arr_baomoiprchungkhoannoibat[i].value);
arr_baomoiprchungkhoannoibat.splice(i, 1);
break;
}
}
}
for (var index = 0; index < Math.min(dist_baomoiprchungkhoannoibat.length, slots_baomoiprchungkhoannoibat.length) ; index++)
slots_baomoiprchungkhoannoibat[index].outerHTML = dist_baomoiprchungkhoannoibat[index];
}
