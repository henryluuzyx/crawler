    <script type="text/javascript">
        $(function () {
            GiftCodePages.init();

            try {
                $(window).scroll(function () {
                    var heightNeo = $("#divRightNeoHeight").height() + 503;
                    if ($('.left-tin2').size() && $('#divRightNeo').size() && $(".left-tin2").height() > $('.right-tin2').height()) {
                        var a = COMMON.getScrollTop();
                        var b = $("#gfooter").position().top - heightNeo  - 20;

                        if (a > ($("#divRightNeoTop").position().top + $("#divRightNeoTop").height())) {
                            if (a >= b) {
                                $("#divRightNeo").css({ position: "absolute", top: b });
                            } else {
                                $("#divRightNeo").css({ position: "fixed", top: "0px" });
                            }
                        } else {
                            $("#divRightNeo").css("position", "static");
                        }
                    } else {
                        $("#divRightNeo").css("position", "static");
                    }
                });

            } catch (e) {

            }

        });
    </script>




