	 <script type="text/javascript">
		var current_url = 'http://e.vnexpress.net/interactive/2017/eight-world-class-resorts-in-vietnam-that-could-cost-you-fortunes';
		function shareFB() {
			href = 'http://www.facebook.com/sharer.php?u=' + current_url;
			window.open(href, '_blank', 'height=450,width=700,resizable=no,status=no');
		}

		function shareGG() {
			href = 'https://plus.google.com/share?url=' + current_url;
			window.open(href, '_blank', 'height=450,width=700,resizable=no,status=no');
		}

		function shareTW() {
			
			
			var shortUrl = '';
			$.getJSON("http://api.bit.ly/v3/shorten?login=o_1g23qvlqab&apiKey=R_0074fe3a89174f07ac13f1b47956f1eb&longUrl="+current_url, function(data) {
				if(typeof(data.data.url) != 'undefined' && data.data.url != '')
				{
					shortUrl = data.data.url;
				}
			});
			setTimeout(function(){
				if(shortUrl)
				{
					var linkTw = "https://twitter.com/intent/tweet?source=webclient&text="+$('title').text()+" "+shortUrl;
					window.open(linkTw,'_blank','height=450,width=700,resizable=no,status=no');
				}
			}, 1000);
							
							
		}

		function urlencode(str) {
			str = (str + '').toString();
			return encodeURIComponent(str).replace(/#!/g, '%23').replace(/!/g, '%21').replace(/'/g, '%27').replace(/\(/g, '%28').replace(/\)/g, '%29').replace(/\*/g, '%2A').replace(/%20/g, '+');
		}
	 </script>
	 <script type="text/javascript">
	 
	 
        // FullPage
        var pageViews = 1;
		var i12 = 0;
        var isView = [];
        var FullPage = function() {
            "use strict";

            // Handle FullPage
            var handleFullPage = function() {
                $('#fullpage').fullpage({
                    anchors: ['page-1', 'page-2', 'page-3', 'page-4', 'page-5', 'page-6', 'page-7', 'page-8', 'last-page'],
                    navigation: true,
                    navigationPosition: 'right',
                    onLeave: function(index, nextIndex, direction){
                        if(direction == 'down')
                        {
							var idPage = $('.fp-completely').attr('id');
							if(idPage == 'section-8'){if(i12 == 0){$('#tracking').append('<iframe src="/interactive/2017/eight-world-class-resorts-in-vietnam-that-could-cost-you-fortunes/tracking.html" width="0" height="0"></iframe>');i12++;}}
                        }
                    }
                });
				$('#to_top').bind('click', function(){
					return $.fn.fullpage.moveTo(1);
				})
            }

            return {
                init: function() {
                    handleFullPage(); // initial setup for fullPage
                }
            }
        }();
        $(document).ready(function() {
            FullPage.init();
        });
		// traking when hove text
		var listId = [];
		$('.warp-text').on('hover', function(){
			var currentPage =  $(this).attr('data-page-id');
			//console.log(currentPage);
			if($.inArray(currentPage,listId) < 0){
				listId.push(currentPage);
				$('#tracking').append('<iframe src="/interactive/2017/eight-world-class-resorts-in-vietnam-that-could-cost-you-fortunes/tracking.html" width="1" height="1"></iframe>');
				//console.log('tracking '+ currentPage);
			}
		});
        </script>

