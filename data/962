          <script type="text/javascript">
            //search danh gia filter
            var count_stt=0;
            $('#filter_web .line_thietbi a.danhgia_search').click(function(){	
            	$('#filter_web .search_filter').slideToggle(100,function(){
            		count_stt++;
            		if((count_stt%2)==0){
            			$('#filter_web .line_thietbi a.danhgia_search').removeClass('danhgia_search_act');	
            		}else{
            			$('#filter_web .line_thietbi a.danhgia_search').addClass('danhgia_search_act');		
            		}
            	});	
            	
            })
