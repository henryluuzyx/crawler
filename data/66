<script>
var arr_baomoiprvanhoanoibat = [
];;
if (arr_baomoiprvanhoanoibat.length > 0) {
var slots_baomoiprvanhoanoibat = new Array;
var prefix = "pr_baomoiprvanhoanoibat_";
for (var i = 1; i <= 10; i++) {
var id = prefix + i;
var obj = document.getElementById(id);
if (obj == null) break;
else slots_baomoiprvanhoanoibat.push(obj);
}
var total = 100.0;
var dist_baomoiprvanhoanoibat = [];
for (var index = 0; index < slots_baomoiprvanhoanoibat.length; index++) {
var t = Math.floor(Math.random() * total);
var n = 0;
for (var i = 0; i < arr_baomoiprvanhoanoibat.length; i++) {
n += arr_baomoiprvanhoanoibat[i].share;
if (n >= t) {
total -= arr_baomoiprvanhoanoibat[i].share;
dist_baomoiprvanhoanoibat.push(arr_baomoiprvanhoanoibat[i].value);
arr_baomoiprvanhoanoibat.splice(i, 1);
break;
}
}
}
for (var index = 0; index < Math.min(dist_baomoiprvanhoanoibat.length, slots_baomoiprvanhoanoibat.length) ; index++)
slots_baomoiprvanhoanoibat[index].outerHTML = dist_baomoiprvanhoanoibat[index];
}
</script>
