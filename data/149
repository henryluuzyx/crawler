var arr_baomoiprkhoahoctunhiennoibat = [
];;
if (arr_baomoiprkhoahoctunhiennoibat.length > 0) {
var slots_baomoiprkhoahoctunhiennoibat = new Array;
var prefix = "pr_baomoiprkhoahoctunhiennoibat_";
for (var i = 1; i <= 10; i++) {
var id = prefix + i;
var obj = document.getElementById(id);
if (obj == null) break;
else slots_baomoiprkhoahoctunhiennoibat.push(obj);
}
var total = 100.0;
var dist_baomoiprkhoahoctunhiennoibat = [];
for (var index = 0; index < slots_baomoiprkhoahoctunhiennoibat.length; index++) {
var t = Math.floor(Math.random() * total);
var n = 0;
for (var i = 0; i < arr_baomoiprkhoahoctunhiennoibat.length; i++) {
n += arr_baomoiprkhoahoctunhiennoibat[i].share;
if (n >= t) {
total -= arr_baomoiprkhoahoctunhiennoibat[i].share;
dist_baomoiprkhoahoctunhiennoibat.push(arr_baomoiprkhoahoctunhiennoibat[i].value);
arr_baomoiprkhoahoctunhiennoibat.splice(i, 1);
break;
}
}
}
for (var index = 0; index < Math.min(dist_baomoiprkhoahoctunhiennoibat.length, slots_baomoiprkhoahoctunhiennoibat.length) ; index++)
slots_baomoiprkhoahoctunhiennoibat[index].outerHTML = dist_baomoiprkhoahoctunhiennoibat[index];
}
</script>
