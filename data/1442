var divshow = '';
var idnow = 'li1';
function ShowMenu(id) {
divshow = 'ul-' + id;
for (var i = 1; i < 100; i++) {
if (document.getElementById('li' + i)) {
if (document.getElementById('ul-li' + i)) {
document.getElementById('ul-li' + i).style.display = 'none';
}
document.getElementById('li' + i).className = 'menuparent-nomal';
}
else {
break;
}
}
if (document.getElementById('ul-' + id)) {
document.getElementById('ul-' + id).style.display = 'block';
}
document.getElementById(id).className = 'menuparent-select';
}
function HiddenMenu() {
if (idnow != '' && document.getElementById(idnow))
ShowMenu(idnow);
}
HiddenMenu();
</script>

