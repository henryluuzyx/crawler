}
catch (e) {
}
}
function setupMedia(obj) {
jwplayer('tno-player').setup({
image: obj.ImageUrl,
file: obj.FileUrl,
skin: 'stormtrooper',
controlbar: { position: 'bottom' },
primary: 'html5',
width: 286,
height: 220, autostart: obj.AutoStart
}); // jwplayer
jwplayer('tno-player').setVolume(1);
}
