        <script language="javascript">
            $('#txtKeyword').bind('keypress', function (e) {
                if (e.keyCode == 13) {
                    if ($("#txtKeyword").val() == '') {
                        alert("Bạn chưa nhập nội dung");
                        $("#txtKeyword").focus();
                    }
                    else {
                        $(window.location).attr('href', '/search/?keyword=' + $("#txtKeyword").val());
                    }
                }
            });
            $('#btnSearch').click(function () {
                if ($("#txtKeyword").val() == '') {
                    alert("Bạn chưa nhập nội dung");
                    $("#txtKeyword").focus();
                }
                else {
                    $(window.location).attr('href', '/search/?keyword=' + $("#txtKeyword").val());
                }
            });
