if (arr_baomoiprlaodongvieclamnoibat.length > 0) {
var slots_baomoiprlaodongvieclamnoibat = new Array;
var prefix = "pr_baomoiprlaodongvieclamnoibat_";
for (var i = 1; i <= 10; i++) {
var id = prefix + i;
var obj = document.getElementById(id);
if (obj == null) break;
else slots_baomoiprlaodongvieclamnoibat.push(obj);
}
var total = 100.0;
var dist_baomoiprlaodongvieclamnoibat = [];
for (var index = 0; index < slots_baomoiprlaodongvieclamnoibat.length; index++) {
var t = Math.floor(Math.random() * total);
var n = 0;
for (var i = 0; i < arr_baomoiprlaodongvieclamnoibat.length; i++) {
n += arr_baomoiprlaodongvieclamnoibat[i].share;
if (n >= t) {
total -= arr_baomoiprlaodongvieclamnoibat[i].share;
dist_baomoiprlaodongvieclamnoibat.push(arr_baomoiprlaodongvieclamnoibat[i].value);
arr_baomoiprlaodongvieclamnoibat.splice(i, 1);
break;
}
}
}
for (var index = 0; index < Math.min(dist_baomoiprlaodongvieclamnoibat.length, slots_baomoiprlaodongvieclamnoibat.length) ; index++)
slots_baomoiprlaodongvieclamnoibat[index].outerHTML = dist_baomoiprlaodongvieclamnoibat[index];
}
