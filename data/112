var total = 100.0;
var dist_baomoiprquanvotnoibat = [];
for (var index = 0; index < slots_baomoiprquanvotnoibat.length; index++) {
var t = Math.floor(Math.random() * total);
var n = 0;
for (var i = 0; i < arr_baomoiprquanvotnoibat.length; i++) {
n += arr_baomoiprquanvotnoibat[i].share;
if (n >= t) {
total -= arr_baomoiprquanvotnoibat[i].share;
dist_baomoiprquanvotnoibat.push(arr_baomoiprquanvotnoibat[i].value);
arr_baomoiprquanvotnoibat.splice(i, 1);
break;
}
}
}
for (var index = 0; index < Math.min(dist_baomoiprquanvotnoibat.length, slots_baomoiprquanvotnoibat.length) ; index++)
