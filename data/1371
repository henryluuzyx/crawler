<script type="text/lazyscript">
    var items = $('.mv-item');
    if (items && items.length > 0) {
        var maxHeight = 0;
        items.each(function () {
            if ($(this).height() > maxHeight) {
                maxHeight = $(this).height();
            }
            if ((items.index($(this))+1) % 4 == 0) {

                $(this).addClass("last");
            }
        });
        items.css('height', maxHeight);
    }
</script>


