    var article_description = 'H%E1%BB%87+th%E1%BB%91ng%C2%A0TechOne+t%E1%BB%95+ch%E1%BB%A9c+ch%C6%B0%C6%A1ng+tr%C3%ACnh+thu+c%C5%A9+%C4%91%E1%BB%95i+m%E1%BB%9Bi%2C+l%C3%AAn+%C4%91%E1%BB%9Di+iPhone+7%2C+7+Plus+v%E1%BB%9Bi+gi%C3%A1+t%E1%BB%AB+3+tri%E1%BB%87u+%C4%91%E1%BB%93ng.';
    var article_image = 'http://img.f26.kinhdoanh.vnecdn.net/2017/03/08/7-3-20175-3506-1488963283_490x294.jpeg';
			
    $(document).ready(function(){
        //init send mail
        sendmail.init({
            article_link: article_url,
            article_title: 'Sở hữu iPhone 7 từ 3 triệu đồng với chính sách \'thu cũ đổi mới\' - VnExpress Kinh Doanh',
