</section>
<script>
var arr_baomoipramthucnoibat = [
];;
if (arr_baomoipramthucnoibat.length > 0) {
var slots_baomoipramthucnoibat = new Array;
var prefix = "pr_baomoipramthucnoibat_";
for (var i = 1; i <= 10; i++) {
var id = prefix + i;
var obj = document.getElementById(id);
if (obj == null) break;
else slots_baomoipramthucnoibat.push(obj);
}
var total = 100.0;
var dist_baomoipramthucnoibat = [];
for (var index = 0; index < slots_baomoipramthucnoibat.length; index++) {
var t = Math.floor(Math.random() * total);
var n = 0;
for (var i = 0; i < arr_baomoipramthucnoibat.length; i++) {
n += arr_baomoipramthucnoibat[i].share;
if (n >= t) {
total -= arr_baomoipramthucnoibat[i].share;
dist_baomoipramthucnoibat.push(arr_baomoipramthucnoibat[i].value);
arr_baomoipramthucnoibat.splice(i, 1);
break;
}
}
}
for (var index = 0; index < Math.min(dist_baomoipramthucnoibat.length, slots_baomoipramthucnoibat.length) ; index++)
slots_baomoipramthucnoibat[index].outerHTML = dist_baomoipramthucnoibat[index];
}
</script>
