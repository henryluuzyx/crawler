<script type="text/javascript">
    var api_upload_url = 'http://photo.upload.vnexpress.net';    
    $(document).ready(function() {
        postarticle.init({
            api_upload: 'http://photo.upload.vnexpress.net/photo',
            session_id: ''
        });
    });
	$(document).ready(function() {
		var r = gup('r', '');
		if(r == 1)
			document.getElementById("slCate").selectedIndex = "2";
	});
	function gup( name, url ) {
	  if (!url) url = location.href
	  name = name.replace(/[\[]/,"\\\[").replace(/[\]]/,"\\\]");
	  var regexS = "[\\?&]"+name+"=([^&#]*)";
	  var regex = new RegExp( regexS );
	  var results = regex.exec( url );
	  return results == null ? null : results[1];
	}
	
</script>
