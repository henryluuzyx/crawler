<script type="text/javascript">
    COMMON.imageCaption('.noidung-p');
    $(function () {
        NewsDetailPages.init(23518);
        InterestUserControl.init(23518);

        try {
            $(window).scroll(function () {
                var heightNeo = $("#divRightNeoHeight").height() + 503;
                if ($('.left-baiviet').size() && $('#divRightNeo').size() && $(".left-baiviet").height() > $('.right-baiviet').height()) {
                    var a = COMMON.getScrollTop();
                    var b = $("#gfooter").position().top - heightNeo  - 20;

                    if (a > ($("#divBannerRight2").position().top + $("#divBannerRight2").height())) {
                        if (a >= b) {
                            $("#divRightNeo").css({ position: "absolute", top: b });
                        } else {
                            $("#divRightNeo").css({ position: "fixed", top: "0px" });
                        }
                    } else {
                        $("#divRightNeo").css("position", "static");
                    }
                } else {
                    $("#divRightNeo").css("position", "static");
                }
            });

        } catch (e) {

        }
    });
</script>


