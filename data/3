<script type="text/javascript">
$(document).ready(function(){
    var owl_slider_photo = $(".block_slider_photo #slider_box_photo");
      owl_slider_photo.owlCarousel({
        autoPlay: 7000,
        items : 1,
        itemsDesktop : [1199,1],
        itemsTablet: [600,1], //2 items between 600 and 0
        itemsDesktopSmall : [900,1],// betweem 900px and 601px
        itemsMobile   :[479,1],
        pagination:false
    });
    $(".block_slider_photo .next_slider").click(function(){
        owl_slider_photo.trigger('owl.next');
    });
    $(".block_slider_photo .prev_slider").click(function(){
        owl_slider_photo.trigger('owl.prev');
    });
});
</script>
