                </div>
            </div>
        </div>
    </div>
    <script type="text/javascript">
        $(document).ready(function() {
            /* Scroll box laisuatnganhang */
            if ($('#laisuatnganhang .tab1 .scroll-pane .nicescroll-rails').size() == 0) {
                $('#laisuatnganhang .tab1 .scroll-pane').niceScroll('#laisuatnganhang .tab1 .content_scoller', { touchbehavior: false, autohidemode: false });
            }
            kinhdoanh.showTab('#laisuatnganhang', function() {
                if ($('#laisuatnganhang .tab1 .scroll-pane .nicescroll-rails').size() == 0) {
                    $('#laisuatnganhang .tab1 .scroll-pane').niceScroll('#laisuatnganhang .tab1 .content_scoller', { touchbehavior: false, autohidemode: false });
                }
                if ($('#laisuatnganhang .tab2 .scroll-pane .nicescroll-rails').size() == 0) {
                    $('#laisuatnganhang .tab2 .scroll-pane').niceScroll('#laisuatnganhang .tab2 .content_scoller', { touchbehavior: false, autohidemode: false });
                }
                if ($('#laisuatnganhang .tab3 .scroll-pane .nicescroll-rails').size() == 0) {
                    $('#laisuatnganhang .tab3 .scroll-pane').niceScroll('#laisuatnganhang .tab3 .content_scoller', { touchbehavior: false, autohidemode: false });
                }
            });
        });
    </script>
