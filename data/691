            itemsDesktopSmall : [900,1],// betweem 900px and 601px
            itemsMobile     :[479,1],
            pagination:false,
                    });
        $("#km_dulich .next").click(function(){
            owl_slider_khuyenmai.trigger('owl.next');
        });
        $("#km_dulich .prev").click(function(){
            owl_slider_khuyenmai.trigger('owl.prev');
        });
    });
