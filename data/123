var arr_baomoiprthoitrangnoibat = [
];;
if (arr_baomoiprthoitrangnoibat.length > 0) {
var slots_baomoiprthoitrangnoibat = new Array;
var prefix = "pr_baomoiprthoitrangnoibat_";
for (var i = 1; i <= 10; i++) {
var id = prefix + i;
var obj = document.getElementById(id);
if (obj == null) break;
else slots_baomoiprthoitrangnoibat.push(obj);
}
var total = 100.0;
var dist_baomoiprthoitrangnoibat = [];
for (var index = 0; index < slots_baomoiprthoitrangnoibat.length; index++) {
var t = Math.floor(Math.random() * total);
var n = 0;
for (var i = 0; i < arr_baomoiprthoitrangnoibat.length; i++) {
n += arr_baomoiprthoitrangnoibat[i].share;
if (n >= t) {
total -= arr_baomoiprthoitrangnoibat[i].share;
dist_baomoiprthoitrangnoibat.push(arr_baomoiprthoitrangnoibat[i].value);
arr_baomoiprthoitrangnoibat.splice(i, 1);
break;
}
}
}
for (var index = 0; index < Math.min(dist_baomoiprthoitrangnoibat.length, slots_baomoiprthoitrangnoibat.length) ; index++)
slots_baomoiprthoitrangnoibat[index].outerHTML = dist_baomoiprthoitrangnoibat[index];
}
</script>
