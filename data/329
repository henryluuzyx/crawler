jQuery(document).ready(function($) {$(".navbar-nav li").last().addClass("live");});

        var frontsiteurl = 'http://tv.tuoitre.vn';
        function tracking(id){
            var track_url = 'http://utils.tuoitre.vn/?r=event/tvo/view&id=';
            track_url = track_url + id;
            $.get(track_url);
        }
        $(document).ready(function() { 
            var path_video = $('#scroller').children('.playing').children('.par_video').val();
            var schedule_temp = $('#scroller').children('.playing').children('.par_schedule').val();
            var path_thumb = $('#scroller').children('.playing').children('.par_thumb').val();
            var schedule_str = JSON.parse(schedule_temp);
            var player_path = '/assets/c347b1f2/js/jwplayer7/player.swf?v=32';

            var clip_id = $('#scroller').children('.playing').children('.clip-id').val();
            tracking(clip_id);
            playVideo(path_video, path_thumb, schedule_str, player_path);
        }); //End ready

        $('.slider_video').click(function(){
                var related = $(this).parent().find('.info').html();
                var tag_li = $('#scroller').children();
                for (i = 0; i < tag_li.length; i++) {
                    $(tag_li[i]).find('.slider_video').removeClass('current');
                    $(tag_li[i]).removeClass('playing');
                }
                $(this).parent().addClass('playing');
                $(this).addClass('current');
                $('#maindesc').find('*').remove();
                $('#maindesc').append(related);
                var path_video = $(this).parent().children('.par_video').val();
                var path_thumb = $(this).parent().children('.par_thumb').val();
                var schedule_temp = $(this).parent().children('.par_schedule').val();
                var schedule_str = JSON.parse(schedule_temp);
                var player_path = '/assets/c347b1f2/js/jwplayer7/player.swf?v=32';

                var clip_id = $(this).parent().children('.clip-id').val();
                tracking(clip_id);
                playVideo(path_video, path_thumb, schedule_str, player_path);
                changeUrl(frontsiteurl);
            });

            //$('#scroller').simplyScroll();
            $('#scroller').children(':first').addClass('playing');
            $('#scroller').children(':first').find('.slider_video').addClass('current');
            $.get('http://utils.tuoitre.vn/?r=event/tvo/view&id=28903');
            changeUrl(frontsiteurl);
    
