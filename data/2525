<script src="http://st.f1.sohoa.vnecdn.net/event/top10smartphone/js/jquery.fullPage.js"></script>
<script type="text/javascript">
function PosBlock(){
    var heightView = $(window).height();
    var widthView = $(window).width();
    var heightBlock = $('.block_list_phone').height();
    var widthBlock = $('.block_list_phone').width();
    
    var posX = (widthView - widthBlock)/2;  
    var posY = (heightView - heightBlock)/2;
    
    if(heightBlock > heightView){
            $('.block_list_phone').css({"left": posX, "top": 0})
        }
        else{
            $('.block_list_phone').css({"left": posX, "top": posY})
        }
}
    if(screen.height>900){
        $('.section').css({
            "top": "30%"
        });
    }
    else{
        $('.section').css({
            "top":"23%", 
        });
    }
    $(function() {
    
    /*V2*/
    $('#page').fullpage({
            'verticalCentered': false,
            'slidesNavigation': true,
            'slidesNavPosition': 'bottom',
            'css3': true,
            'navigation': false,
            'responsive' : 300,
            'navigationPosition': 'right',
            'menu': '#list_pagination',
            'anchors':['10rdPage', '9rdPage', '8rdPage'],
            'slidesNavigation': true,
            'autoScrolling': true,
            'navigationTooltips': ['text 1', 'text 2', 'text 3', 'text 4'],

            /*'afterLoad': function(anchorLink, index){
                if(index == 2){
                    $('.title_top5').addClass('active');
                }
            },

            'onLeave': function(index, nextIndex, direction){
                if (index == 3 && direction == 'down'){
                    $('.section').eq(index -1).removeClass('moveDown').addClass('moveUp');
                }
                else if(index == 3 && direction == 'up'){
                    $('.section').eq(index -1).removeClass('moveUp').addClass('moveDown');
                }

                $('#staticImg').toggleClass('active', (index == 2 && direction == 'down' ) || (index == 4 && direction == 'up'));
                $('#staticImg').toggleClass('moveDown', nextIndex == 4);
                $('#staticImg').toggleClass('moveUp', index == 4 && direction == 'up');
            }*/
        });
    /*End V2*/
    
    
    $('.block_list_phone .phone_item').click(function(){
        $('#page').addClass('section2');
        $('#page').removeClass('show_txt');
        $('.back_home').addClass('active');
        cycleItems();
    })
    $('.go_home').click(function(){     
        $('#page').removeClass('section2');
        $('#page').removeClass('show_txt');
        $('.page_slider').removeClass('view_thumb');
        $('.block_main_slider').removeClass('running');
    })
    $('.view_all').click(function(){
       $(this).addClass('active'); 
        $('.view_total').addClass('view_cc');
        $('#page').addClass('show_txt');
        $('.section').removeClass('running');
        $('.section').removeClass('active');
        $('.back_home').removeClass('active');
        $('#list_pagination').find('li').removeClass('active');
    })
    $('.show_item').click(function(){
        $('.page_slider').removeClass('view_thumb');
        $('#page').removeClass('show_txt');
        $('.back_home').addClass('active');
        $('.view_all').removeClass('active');
        //$('.section').addClass('active');
        $('.block_main_slider').removeClass('running');
        $('.view_total').removeClass('view_cc');
    })
    $('.pagination_item').click(function(){
        $('.view_total').removeClass('view_cc');
    })

});

</script>
<script type="text/javascript">
    $(".icon_hover").mouseover(function(event) {
        event.preventDefault();
        $(this).parent().addClass("active");
        $(this).parent().siblings().removeClass("active");
    });
    
    $(".icon_hover").mouseout(function(event) {
        event.preventDefault();
        $(this).parent().removeClass("active");
    });
</script>
