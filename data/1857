<script>
//window.onload = function () {
// getImageReport();
//};
getImageReport();
function getImageReport() {
var selectTop = document.getElementById("selectTop");
var selectField = document.getElementById("selectField");
var selectDaysAgo = document.getElementById("selectDaysAgo");
if (selectTop != null && selectTop != undefined && selectField != null && selectField != undefined && selectDaysAgo != null && selectDaysAgo != undefined) {
var valueTop = selectTop.value;
var valueField = selectField.value;
var valueDaysAgo = selectDaysAgo.value;
var chartImageReport = document.getElementById("chartImageReport");
if (chartImageReport != null && chartImageReport != undefined) {
chartImageReport.src = "imagechart.aspx?top=" + valueTop + "&field=" + valueField + "&daysago=" + valueDaysAgo;
var chartImageMap = document.getElementById("chartImageMap");
if (chartImageMap != null && chartImageMap != undefined) {
if (chartImageMap.areas.length > 0) {
for (var i = chartImageMap.areas.length - 1; i >= 0; i--) {
chartImageMap.areas[i].remove();
}
}
var itemheight = 17;
var itemclear = 12;
var itemwidth = 502;
for (var i = 0; i < valueTop; i++) {
addArea(chartImageMap, 0, (i + 1) * itemclear + i * itemheight, itemwidth, (i + 1) * (itemclear + itemheight), "imagechart.aspx?top=" + valueTop + "&field=" + valueField + "&daysago=" + valueDaysAgo + "&item=" + i, "Xem chi tiết");
}
}
chartImageReport.style.display = 'block';
}
}
};
function addArea(map, xstart, ystart, xende, yende, link, title) {
area = document.createElement("area");
area.shape = "rect";
area.coords = "" + xstart + ", " + ystart + ", " + xende + ", " + yende + "";
area.title = title;
area.alt = title;
//area.onmouseover = function () {
// console.log(title + ": " + link);
// return false;
//};
area.onclick = function () {
viewPopup(link);
return false;
};
map.appendChild(area);
};
</script>
<script>
function viewPopup(url) {
var e = $("#dialog");
0 == e.length && $("body").append($("<div></div>", {
id: "dialog"
}));
$("#dialog").css("display", "block");
if ($("#charts-modal").length > 0)
$("#charts-modal").remove();
$("#dialog").append('<div id="charts-modal" class="reveal-modal charts-modal large" style="width:542px; height:360px; margin-top:-200px !important; margin-left:-271px !important;"></div>');
$("#charts-modal").append('<div class="header"><span>Dữ liệu thống kê chi tiết trong vòng 7 ngày gần nhất</span></div>');
$(".header").append('<a class="close-reveal-modal" onclick="ClosePopup()">×</a>');
$("#charts-modal").append('<div id="charts-content" class="content">');
$("#charts-content").append('<img src="' + url + '" style="border: 1px black solid;" alt="Không có dữ liệu hiển thị" />');
$('#dialog').click(function (e) {
var offset_t = $("#charts-modal").offset().top - $(window).scrollTop();
var offset_l = $("#charts-modal").offset().left - $(window).scrollLeft();
var left = Math.round((e.clientX - offset_l));
var top = Math.round((e.clientY - offset_t));
if (left < 0 || left > 542 || top < 0 || top > 320)
ClosePopup();
});
};
ClosePopup = function () {
if ($('#dialog').length > 0)
$('#dialog').remove();
};
</script>
