var VnExpress = {
    nl2br: function (str, is_xhtml) {
        var breakTag = (is_xhtml || typeof is_xhtml === 'undefined') ? '<br ' + '/>' : '<br>'; // Adjust comment to avoid issue on phpjs.org display
        return (str + '').replace(/([^>\r\n]?)(\r\n|\n\r|\r|\n)/g, '$1' + breakTag + '$2');
    },
    stripUnicode: function(str, symbol)
    {
        if(typeof(str) == 'undefined') return '';

        str= str.toLowerCase();
        str= str.replace(/à|á|ạ|ả|ã|â|ầ|ấ|ậ|ẩ|ẫ|ă|ằ|ắ|ặ|ẳ|ẵ/g,"a");
        str= str.replace(/è|é|ẹ|ẻ|ẽ|ê|ề|ế|ệ|ể|ễ/g,"e");
        str= str.replace(/ì|í|ị|ỉ|ĩ/g,"i");
        str= str.replace(/ò|ó|ọ|ỏ|õ|ô|ồ|ố|ộ|ổ|ỗ|ơ|ờ|ớ|ợ|ở|ỡ/g,"o");
        str= str.replace(/ù|ú|ụ|ủ|ũ|ư|ừ|ứ|ự|ử|ữ/g,"u");
        str= str.replace(/ỳ|ý|ỵ|ỷ|ỹ/g,"y");
        str= str.replace(/đ/g,"d");
        if(typeof(symbol) == 'undefined')
        {
            str= str.replace(/!|@|\$|%|\^|\*|\(|\)|\+|\=|\<|\>|\?|\/|,|\.|\:|\'| |\"|\&|\#|\[|\]|~/g,"-");
            str= str.replace(/-+-/g,"-");
            str= str.replace(/^\-+|\-+$/g,"");
            str= str.replace(/-/g, '_');
        }
        return str.replace(/\s\s+/g, ' ');
    },
    toInt : function(str){
        if(typeof(str) == 'undefined') return 0;
        str = parseInt(str.replace('.', '').trim());
        return  parseInt(str);
    },
    shuffleArray: function(array) {
        for (var i = array.length - 1; i > 0; i--) {
            var j = Math.floor(Math.random() * (i + 1));
            var temp = array[i];
            array[i] = array[j];
            array[j] = temp;
        }
        return array;
    },
    randCarByBrand: function(brands,listCarsByBrand){
        var arrayBrand = $.map(brands, function(value, index) {return [value];});
        var brands     = VnExpress.shuffleArray(arrayBrand);
        var listCarsSorted  = [];
        $.each(brands, function(index,brand) {
            var carOfThisBrand =  listCarsByBrand[brand];
            var arraycarOfThisBrand = $.map(carOfThisBrand, function(value, index) {return [value];});
            carOfThisBrand = arraycarOfThisBrand.sort(function(a,b){
                var nameA = a.carName.toUpperCase();
                var nameB = b.carName.toUpperCase();
                if (nameA < nameB) return -1;
                if (nameA > nameB) return 1;
                return 0;
            });

            $.each(carOfThisBrand,function(index,car){
                listCarsSorted.push(car);
            })
        });
        return listCarsSorted;
    }
}

function showData(data,brand, brand_value, price_value,type_value,source_value, price_filter_deviation)
{
    var html = '';
    var price_value_arr = [];
    if (price_value.indexOf('?') > -1) {
        var price_value_split = price_value.split('?');
        price_value = price_value_split[0];
    }
    if(typeof(price_value) != 'undefined' && price_value != 'all')
    {
        price_value_arr = price_value.split('-');
        price_value_arr[0] = parseInt(price_value_arr[0].trim());
        price_value_arr[1] = parseInt(price_value_arr[1].trim());
    }

    var price_filter_deviation_arr = [];
    if(typeof(price_filter_deviation) != 'undefined' && price_filter_deviation != 'all')
    {
        price_filter_deviation_arr = price_filter_deviation.split('-');
        price_filter_deviation_arr[0] = parseInt(price_filter_deviation_arr[0].trim());
        price_filter_deviation_arr[1] = parseInt(price_filter_deviation_arr[1].trim());
    }

    var filters     = {'brand':brand_value,'price':price_value_arr,'price_deviation':price_filter_deviation_arr ,'type':type_value,'source':source_value};
    var dataResult  = [];

    $.each(data, function(index, object){
        var objectValue = object.product_value;
        var check = true;
        $.each(filters, function(keyFilter, valueFilter){
            switch (keyFilter) {
                case "brand":
                    if(typeof(valueFilter) != 'undefined' && valueFilter !='all'){
                        if(check && VnExpress.stripUnicode(valueFilter) != VnExpress.stripUnicode(object.carBrand)){
                            check = false;
                        }
                    }
                    break;
                case "price":
                    if (typeof price_value_arr[0] !== 'undefined' && price_value_arr[0] !== null) {
                        if(check && VnExpress.toInt(object.carPrice) <= parseInt(price_value_arr[0]) || VnExpress.toInt(object.carPrice) >= parseInt(price_value_arr[1])){
                            check = false;
                        }
                    }
                    break;
                case "price_deviation":
                    if (typeof price_filter_deviation_arr[0] !== 'undefined' && price_filter_deviation_arr[0] !== null) {
                        if(check && VnExpress.toInt(object.carPriceDeviation) <= price_filter_deviation_arr[0] || VnExpress.toInt(object.carPriceDeviation) >= price_filter_deviation_arr[1]){
                            check = false;
                        }
                    }
                    break;
                case "type":
                    if(typeof(valueFilter) != 'undefined' && valueFilter !='all'){
                      if(check && VnExpress.stripUnicode(valueFilter) != VnExpress.stripUnicode(object.carType)){
                          check = false;
                      }
                    }
                    break;                 
                case "source":
                    if(typeof(valueFilter) != 'undefined' && valueFilter !='all'){
                        if(check && VnExpress.stripUnicode(valueFilter) != VnExpress.stripUnicode(object.carOrigin)){
                            check = false;
                        }
                    }
                    break;
            }
        });

        if(check) dataResult.push(object);
    });

    $.each(dataResult, function(k, v){
       if(typeof(v.carName) != 'undefined'){
            html += '<tr class="bg_tr up_level brand '+v.carBrand+'" data-url="'+v.shareUrl+'" data-brand="'+v.carBrand.trim().toLowerCase()+'" data-deviation="'+ v.carPriceDeviation.replace(/\./g, '') +'">\
                        <td class="td1_2 uplv">\
                            <a class="name_xe" href="javascript:;" >'+v.carName+'\
                                <div class="show_xemchitiet">Xem chi tiết</div>\
                            </a>\
                        </td>\
                        <td class="td9">'+v.carBrand+'</td>\
                        <td class="td9">'+v.carType+'</td>\
                        <td class="td9">'+v.carOrigin+'</td>\
                        <td class="td9" style="color:#9f224e;font-weight:bold;">'+v.carPrice+'</td>\
                        <td class="td9" style="color:#58a12b;font-weight:bold;">'+v.carPriceDeviation+'</td>\
                        <td class="td9">'+v.carEngine+'</td>\
                        <td class="td9">'+v.carPower+'</td>\
                        <td class="td9">'+v.carMoment+'</td>\
                    </tr>';
        }
    });
    $('table.tbl-ct tbody').html(html).hide().fadeIn('fast', function(){
        tabProduct('mmid');   
        $('.scroll_box').jScrollPane({ showArrows: true });
        var line = [];
        var background = '#eeeeee';
        $('tr.brand').each(function(index, el) {
            line[index] = $(el).data('brand');
            if(line[index] != line[index-1]) {
                background = (background == '#eeeeee') ? '#ffffff' : '#eeeeee';
            }
            $('tr.brand[data-brand="'+ line[index] +'"]').css({
                'background': background,
                'border-top': '1px solid #e5e5e5'
            });
        });
    });
    var current_url = window.location.href.split('#');
    window.history.pushState("", "", current_url[0]+'#'+brand_value+';'+price_value);
    $('#brand_filter, #price_filter').val(brand_value);
    $('#price_filter').val(price_value);
    // click row
    $('.up_level').css({'cursor':'pointer'});
    $('.up_level').bind('click', function(){
        var url = $(this).attr('data-url');
        if(typeof(url) != 'undefined')
        {
            var win = window.open(url, '_blank');
            win.focus();
        } 
    });
    return true;
}
$(document).ready(function(){
    // get state
    var data = VnExpress.randCarByBrand(brand,listCarsByBrand);
    var current_url = window.location.href;
    current_url = current_url.split('#');
    if(typeof(current_url[1]) != 'undefined')
    {
        var v = current_url[1].split(';');
        if(typeof(v[0]) != 'undefined' && typeof(v[1]) != 'undefined')
        {
            if(v[0].trim() == '')
            {
                v[0] = 'all';
            }
            if(v[1].trim() == '')
            {
                v[1] = 'all';
            }
            showData(data, brand, v[0], v[1]);    
        }
    }
    else
    {
        showData(data, brand, 'all', 'all');    
    }
    // set state
    $('#brand_filter, #price_filter, #type_filter, #source_filter, #price_filter_deviation').bind('change', function(){
        var brand_value = $('#brand_filter').val();
        var price_value = $('#price_filter').val();
        var type_value = $('#type_filter').val();
        var source_value = $('#source_filter').val();
        var price_filter_value = $('#price_filter_deviation').val();
        showData(data, brand, brand_value, price_value,type_value,source_value,price_filter_value);
    });

});
</script>
