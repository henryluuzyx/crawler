<script>
var arr_baomoiprhinhsudansunoibat = [
];;
if (arr_baomoiprhinhsudansunoibat.length > 0) {
var slots_baomoiprhinhsudansunoibat = new Array;
var prefix = "pr_baomoiprhinhsudansunoibat_";
for (var i = 1; i <= 10; i++) {
var id = prefix + i;
var obj = document.getElementById(id);
if (obj == null) break;
else slots_baomoiprhinhsudansunoibat.push(obj);
}
var total = 100.0;
var dist_baomoiprhinhsudansunoibat = [];
for (var index = 0; index < slots_baomoiprhinhsudansunoibat.length; index++) {
var t = Math.floor(Math.random() * total);
var n = 0;
for (var i = 0; i < arr_baomoiprhinhsudansunoibat.length; i++) {
n += arr_baomoiprhinhsudansunoibat[i].share;
if (n >= t) {
total -= arr_baomoiprhinhsudansunoibat[i].share;
dist_baomoiprhinhsudansunoibat.push(arr_baomoiprhinhsudansunoibat[i].value);
arr_baomoiprhinhsudansunoibat.splice(i, 1);
break;
}
}
}
for (var index = 0; index < Math.min(dist_baomoiprhinhsudansunoibat.length, slots_baomoiprhinhsudansunoibat.length) ; index++)
slots_baomoiprhinhsudansunoibat[index].outerHTML = dist_baomoiprhinhsudansunoibat[index];
}
</script>
