<script type="text/javascript">
var leftboxbot, rightboxtop, rightboxbot;
var rightstickyheight = $('#rightsticky').height();
function updatestickypos() {
rightstickyheight = $('#rightsticky').height();
rightboxtop = $('#rightstickyanchor').position().top - 40;
rightboxbot = $('section.zone-media').position().top - rightstickyheight - 40;
}
$(document).ready(function () {
if (!ismobile) {
updatestickypos();
window.setInterval(function () { updatestickypos(); }, 3000);
$(window).on("load", function () { updatestickypos(); });
}
$(window).scroll(function () {
var mytop = $(window).scrollTop();
//floating left box
if (!ismobile) {
if (mytop >= rightboxtop) {
if (mytop <= rightboxbot - 40) {
if ($('#rightsticky').css('position') != 'fixed') { $('#rightsticky').css({ 'position': 'fixed', 'top': '40px' }).width('160px'); }
} else {
if ($('#rightsticky').css('position') != 'absolute') { $('#rightsticky').css({ 'position': 'absolute', 'top': rightboxbot + 'px' }); }
}
} else {
if ($('#rightsticky').css('position') != '') { $('#rightsticky').css('position', ''); }
}
}
});
});
</script>

