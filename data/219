var arr_baomoiprkhonggiankientrucnoibat = [
];;
if (arr_baomoiprkhonggiankientrucnoibat.length > 0) {
var slots_baomoiprkhonggiankientrucnoibat = new Array;
var prefix = "pr_baomoiprkhonggiankientrucnoibat_";
for (var i = 1; i <= 10; i++) {
var id = prefix + i;
var obj = document.getElementById(id);
if (obj == null) break;
else slots_baomoiprkhonggiankientrucnoibat.push(obj);
}
var total = 100.0;
var dist_baomoiprkhonggiankientrucnoibat = [];
for (var index = 0; index < slots_baomoiprkhonggiankientrucnoibat.length; index++) {
var t = Math.floor(Math.random() * total);
var n = 0;
for (var i = 0; i < arr_baomoiprkhonggiankientrucnoibat.length; i++) {
n += arr_baomoiprkhonggiankientrucnoibat[i].share;
if (n >= t) {
total -= arr_baomoiprkhonggiankientrucnoibat[i].share;
dist_baomoiprkhonggiankientrucnoibat.push(arr_baomoiprkhonggiankientrucnoibat[i].value);
arr_baomoiprkhonggiankientrucnoibat.splice(i, 1);
break;
}
}
}
for (var index = 0; index < Math.min(dist_baomoiprkhonggiankientrucnoibat.length, slots_baomoiprkhonggiankientrucnoibat.length) ; index++)
slots_baomoiprkhonggiankientrucnoibat[index].outerHTML = dist_baomoiprkhonggiankientrucnoibat[index];
}
</script>
