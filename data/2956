        <script type="text/javascript" src="http://st.f3.vnecdn.net/responsive/j/v2/event/30namdoimoi/js/jquery.arctext.js"></script>
        <script type="text/javascript" src="http://st.f3.vnecdn.net/responsive/j/v2/event/30namdoimoi/js/scrolltop.js"></script>
        <script src="http://st.f3.vnecdn.net/responsive/j/v2/event/30namdoimoi/js/owl.carousel.min.js"></script>
        <script type="text/javascript">
        $(function() {
            var owl_slide2 = $("#slider_box_photo");
            owl_slide2.owlCarousel({
                loop: false,
                navigation: true,
                autoPlay: 5000,
                items: 1,
                itemsDesktop: [1199, 1],
                itemsTablet: [600, 1], //2 items between 600 and 0
                itemsDesktopSmall: [900, 1], // betweem 900px and 601px
                itemsMobile: [479, 1],
                pagination: false
            });
            var $example1 = $('#example1').hide();
            $example1.show().arctext({
                radius: 1000,
                dir: -1
            });
            $(".block_close_menu").swipe( {
                //Generic swipe handler for all directions
                swipeLeft:function(event, direction, distance, duration, fingerCount) {
                    $('#box_col_left').animate({left:"-240"},200,function(){
                            var heightColleft = $('#box_col_left .block_scoll_menu').height();
                            $('#page').height('');
                            $('#page').removeClass("hidden");
                            $(".block_close_menu").hide();
                            $("#box_col_left .block_scoll_menu").fadeOut(100);
                        })
                },
                tap:function(event, target) {
                    $('#box_col_left').animate({left:"-240"},200,function(){
                        var heightColleft = $('#box_col_left .block_scoll_menu').height();
                        $('#page').height('');
                        $('#page').removeClass("hidden");
                        $(".block_close_menu").hide();
                        $("#box_col_left .block_scoll_menu").fadeOut(100);
                    })
                },
                //Default is 75px, set to 0 for demo so any distance triggers swipe
                threshold:0
            });
            $('.btn_control_col_left').click(
                function(){
                    var pos1 = $('#box_col_left').css('left');
                    if( pos1 == "-240px"){
                        $("#box_col_left .block_scoll_menu").fadeIn(50);
                        $('#box_col_left').animate({left:"0"},200,function(){
                            var heightColleft = $('#box_col_left .block_scoll_menu').height()+50;
                            $('#page').height('');
                            $('#page').addClass("hidden");
                            $(".block_close_menu").show();

                        })
                    }
                    else
                    {
                        $('#box_col_left').animate({left:"-240"},150,function(){
                            var heightColleft = $('#box_col_left .block_scoll_menu').height();
                            $('#page').height('');
                            $('#page').removeClass("hidden");
                            $(".block_close_menu").hide();
                            $("#box_col_left .block_scoll_menu").fadeOut(50);
                        })
                    }
                }
            );
            $(".block_scoll_menu").niceScroll({
                touchbehavior:false,
                autohidemode:true,
                cursorcolor: "#666",
                cursorwidth: "2px",
                cursorborder: "none",
                zindex: "99"
            });
            $( "#to_be_continue a" ).bind( "click", function() {
                categoryList.paging();
            });
            var textShow = 'Thứ bảy, 18/3/2017 | 08:30 GMT+7';
            var textShowUnderBanner = '<strong>30 năm đổi mới</strong>';
            $('.date').text(textShow);
            if(device_env == 4)
            {
                var showBanner = $('#header_web').position().top + $('#header_web').height();
                $(window).scroll(function() {
                    if($(window).scrollTop() + $(window).height() > $(document).height() - 200) {
                       //categoryList.paging();
                    }
                    if($(window).scrollTop() > showBanner)
                    {
                        $('.date').html(textShowUnderBanner);
                    }
                    else
                    {
                        $('.date').html(textShow);
                    }
                });
            }
            VnExpress.social();
        });
        var VnExpress = {
            social: function()
            {
                $('.fb_share, .tw_share, .gg_share').on('click', function(){
                    var action = $(this).attr('data-type');
                    if(typeof(action) != 'undefined' && action)
                    {
                        if(action == 'fb_share')
                        {
                            var linkFB = "https://www.facebook.com/sharer/sharer.php?u=" + current_url;
                            window.open(linkFB,'_blank','height=450,width=700,resizable=no,status=no');
                        }
                        else if(action == 'tw_share')
                        {
                            var shortUrl = '';
                            $.getJSON("http://api.bit.ly/v3/shorten?login=o_1g23qvlqab&apiKey=R_0074fe3a89174f07ac13f1b47956f1eb&longUrl="+current_url, function(data) {
                                if(typeof(data.data.url) != 'undefined' && data.data.url != '')
                                {
                                    shortUrl = data.data.url;
                                }
                            });
                            setTimeout(function(){
                                if(shortUrl)
                                {
                                    var linkTw = "https://twitter.com/intent/tweet?source=webclient&text="+$('title').text()+" - VnExpress "+shortUrl;
                                    window.open(linkTw,'_blank','height=450,width=700,resizable=no,status=no');
                                }
                            }, 1000);
                        }
                        else if(action == 'gg_share')
                        {
                            var linkGG = "https://plus.google.com/share?url="+current_url;
                            window.open(linkGG,'_blank','height=450,width=550,resizable=no,status=no');
                        }
                    }
                });
                if(typeof(Parser) != "undefined")
                {
                    Parser.SITE_URL         = base_url;
                    Parser.URL              = js_url;
                    Parser.FLASH_URL        = flash_url;
                    Parser.SITE_ID          = site_id;
                    Parser.AUTO_PLAY        = 1;
                    Parser.parseAll();
                }
                $(window).scroll(function() {
                    if($(window).scrollTop() >= 200)
                    {
                        $('#to_top').fadeIn();
                    }
                    else
                    {
                        $('#to_top').fadeOut();
                    }
                });
                // fix menu mobile
                $('.btn_control_col_left:first').attr('style', 'display:none !important');
                $('.block_search .input_search').attr('style', 'width:174px !important');
                $("#box_col_left .block_scoll_menu").css('overflow', 'hidden');
                $('#box_tinlienquan .title_box_category').removeAttr('class').attr('class', 'title_box_category width_common title_box_category_bg_grey').css('background', '#eee');
                $('#box_tinlienquan .title_box_category .txt_main_category a').attr('href', 'javascript:void(0);');
                $('#box_tinlienquan .txt_view_more').attr('href', '/tin-tuc/su-kien/30-nam-doi-moi');
                return true;
            }
        }

        var categoryList = (function(){
            return {
                paging: function()
                {
                    $(window).unbind('scroll');
                    $("#to_be_continue a").text('Đang tải');
                    var htmlarea        =   $("#to_be_continue a").attr('data-container');
                    var category_id     =   $("#to_be_continue a").attr('data-category');
                    var page            =   $("#to_be_continue a").attr('data-page');
                    var url             =   $("#to_be_continue a").attr('data-url');
                    var param = {
                        category_id: category_id,
                        page: page
                    };
                    $.get(url, param, function(data) {
                        if(data.error == 1)
                        {
                            $('#'+htmlarea).append(data.html);
                            $("#to_be_continue a").attr('data-page', data.page);
                            $("#to_be_continue a").text('Xem tiếp');
                            CmtWidget.mix();
                            if(device_env == 4)
                            {
                                $(window).scroll(function() {
                                    if($(window).scrollTop() + $(window).height() > $(document).height() - 200) {
                                       //categoryList.paging();
                                    }
                                });
                            }
                        }
                        else if(data.error == 2)
                        {
                            $("#to_be_continue").remove();
                        }
                    },"json");
                }
            }
        })();
        </script>
