<script language="javascript" type="text/javascript" src="http://st.f2.sohoa.vnecdn.net/j/v19/module/jquery.slideto.min.js"></script>
<script type="text/javascript">
    var photoPaging = {
        currPage: 0,
        __gotoPage: function(page){
            this.currPage = page;
            var keyCache = 'photo-' + this.currPage;
            var cachedContent = null;
            /* Get cache by jcache */
            try{
                cachedContent = $.jCache.getItem(keyCache);
            }
            catch(ex){
                cachedContent = null;
            }
            /* If miss cache */
            if (cachedContent == null || typeof cachedContent == 'undefined')
            {
                $.ajax({
                    type: 'get',
                    url: base_url + '/category/ajaxlistphoto',
                    data: {
                        page: page
                    },
                    dataType: 'json',
                    async: false,
                    beforeSend: function(){
                        //$('#' + video.listVideoEleId).html($('#' + video.loadingEleId).html());
                    },
                    success:function(response)
                    {
                        cachedContent = response;
                        $.jCache.setItem(keyCache, response);
                    }
                });
            }
            //console.log(cachedContent);
            if(!cachedContent.error){
                $('#list_photo').fadeOut('fast', function(){
                    $(this).html(cachedContent.html).fadeIn('fast', function(){
                        if('undefined' != typeof CmtWidget){
                            CmtWidget.mix();
                        }
                    });
                });
            }
            $("#list_photo").slideto({
                highlight: false,
                highlight_color: false,
                highlight_duration: 'slow',
                offset : -20,
                slide_duration : 500
            });
        },
        numPage: function(page){
            this.__gotoPage(page);
        },
        prevPage: function(){
            this.__gotoPage(this.currPage - 1);
        },
        nextPage: function(){
            this.__gotoPage(this.currPage + 1);
        }
    };
    function photoPagingMobile(){
        $('.xemtiep a').click(function(e){
            var link  = $(this);
            var url = link.attr('href');
            var page = link.attr('data-page');
            var obj = link.attr('data-container');
            link.html('Đang tải...');
            $.getJSON(url + '?page=' + page, function(data ) {
                if(data.error != 1){
                    $(obj).append(data.html);
                    link.attr('data-page', parseInt(page) + 1);
                    link.html('Xem tiếp');
                }else{
                    link.parent().remove();
                }
            });
            e.preventDefault();
            e.stopPropagation();
            return false;
        });
    }
    $(document).ready(function(){
        photoPagingMobile();
    });
</script>﻿    </div>
