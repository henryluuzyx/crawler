        $('#sort_bottom td a, #sort_top .sort_top_child a').each(function(key, val){
            var col = $(this).parent().attr('data-sort');
            if(col == 'education' || col == 'birthday')
            {
                $(this).append('<br /><span class="sxep '+col+'"></span>');
            }
            else
            {
                $(this).append('<span class="sxep '+col+'"></span>');
