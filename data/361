<!-- Modal -->
<div class="modal modal-style fade" id="askModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title"><span>Đặt câu hỏi</span></h4>
      </div>
      <div class="modal-body">
        <form id="ask-question" class="frm-1">
            <div class="form-group">
                <label for="name">Họ và tên</label>
                <input type="text" class="form-control" name="fullname">
            </div>
            <div class="form-group">
                <label for="email">Địa chỉ email</label>
                <input type="email" class="form-control" name="email">
            </div>
            <div class="form-group">
                <label for="txtQ">Đặt câu hỏi</label>
                <textarea name="txtQ" class="form-control" rows="4"></textarea>
            </div>
            <div class="g-recaptcha" data-sitekey="6LchWhoTAAAAALVqDBLJbclkSUkZN-6AcRyJ20bj" style="margin-bottom: 13px;"></div>
            <div class="text-center">
                <button class="btn-search">Gửi</button>
            </div>
        </form>
      </div>

    </div><!-- /.modal-content -->
  </div><!-- /.modal-dialog -->
</div><!-- /.modal -->

<script type="text/javascript">

function email_validation(email) {
    var re = /^(([^<>()[\]\\.,;:\s@"]+(\.[^<>()[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
    return re.test(email);
}

function checkInputValidation(name, email, question){
    if(name == ''){
        $('#ask-question input[name=fullname]').css({'border-color':'red'});
        return false;
    } else {
        $('#ask-question input[name=fullname]').css({'border-color':'#ccc'});
    }
    if(email == '' || !email_validation(email)){
        $('#ask-question input[name=email]').css({'border-color':'red'});
        return false;
    } else {
        $('#ask-question input[name=email]').css({'border-color':'#ccc'});
    }
    if(question == ''){
        $('#ask-question textarea[name=txtQ]').css({'border-color':'red'});
        return false;
    } else {
        $('#ask-question textarea[name=txtQ]').css({'border-color':'#ccc'});
    }

    return true;
}

$('#ask-question .btn-search').click(function(){
    var url = 'http://tuyensinh.tuoitre.vn' + '/site/recaptcha';
    var name = $('#ask-question input[name=fullname]').val();
    var email = $('#ask-question input[name=email]').val();
    var question = $('#ask-question textarea[name=txtQ]').val();
    var resCaptcha = $('#ask-question textarea[name=g-recaptcha-response]').val();

    if(checkInputValidation(name, email, question)){
        //console.log(url + "|" + name + "|" + email + "|" + question + "|" + resCaptcha);
        $.ajax({
            url: url,
            type: 'post',
            data: {name:name, email:email, question:question, resCaptcha: resCaptcha},
            success: function(msg){
                msg = JSON.parse(msg);
                if(msg.code == 1){
                    alert('Câu hỏi đã được gửi về tòa soạn. Cảm ơn sự quan tâm của bạn.');
                } else {
                    console.log(msg.message);
                }
            },
            error: function (xhr, status, err){
                console.log('Ask question error: ', status, err.toString());
            }
        });
    }
    return false;
});

</script>

