<script type="text/javascript">
    $(function () {
        CategoryPages.init(31);


        try {
            $(window).scroll(function () {
                var heightNeo = $("#divRightNeoHeight").height() + 503;
                if ($('.left-tin2').size() && $('#divRightNeo').size() && $(".left-tin2").height() > (heightNeo + 260)) {
                    var a = COMMON.getScrollTop();
                    var b = $("#gfooter").position().top - heightNeo  - 20;

                    if (a > ($("#divBannerRight1").position().top + 260)) {
                        if (a >= b) {
                            $("#divRightNeo").css({ position: "absolute", top: b });
                        } else {
                            $("#divRightNeo").css({ position: "fixed", top: "0px" });
                        }
                    } else {
                        $("#divRightNeo").css("position", "static");
                    }
                } else {
                    $("#divRightNeo").css("position", "static");
                }
            });

        } catch (e) {

        }

    });
</script>



