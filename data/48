for (var i = 1; i <= 10; i++) {
var id = prefix + i;
var obj = document.getElementById(id);
if (obj == null) break;
else slots_baomoiprmoitruongkhihaunoibat.push(obj);
}
var total = 100.0;
var dist_baomoiprmoitruongkhihaunoibat = [];
for (var index = 0; index < slots_baomoiprmoitruongkhihaunoibat.length; index++) {
var t = Math.floor(Math.random() * total);
var n = 0;
for (var i = 0; i < arr_baomoiprmoitruongkhihaunoibat.length; i++) {
n += arr_baomoiprmoitruongkhihaunoibat[i].share;
if (n >= t) {
total -= arr_baomoiprmoitruongkhihaunoibat[i].share;
dist_baomoiprmoitruongkhihaunoibat.push(arr_baomoiprmoitruongkhihaunoibat[i].value);
arr_baomoiprmoitruongkhihaunoibat.splice(i, 1);
break;
}
}
}
for (var index = 0; index < Math.min(dist_baomoiprmoitruongkhihaunoibat.length, slots_baomoiprmoitruongkhihaunoibat.length) ; index++)
slots_baomoiprmoitruongkhihaunoibat[index].outerHTML = dist_baomoiprmoitruongkhihaunoibat[index];
}
