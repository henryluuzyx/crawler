                                        if (device_env != 1 && typeof(common.resizeImageDetail) != 'undefined') {
                                            common.resizeImageDetail();
                                            $(window).resize(function() {
                                                common.delayFireOnce(1000).done(function() {
                                                    common.resizeImageDetail();
                                                });
                                            });
                                        }
                                    </script>
