        <div class="box_width_common" id="box_details_news">
                            <div class="w670 left">
                    <div class="main_content_detail width_common">
                        <div class="width_common line_col_480 space_bottom_20">
                            <div class="block_col_480">
                                <div id="left_calculator">
                                                                            <div class="block_timer_share">
                                            <div class="block_timer left txt_666">Thứ tư, 13/1/2016<span class="drash"> | </span>06:29 GMT+7</div>
                                            <div class="block_share right"><a class="btn_facebook" rel="nofollow" href="javascript:;" title="Chia sẻ bài viết lên facebook"><i class="ic ic-facebook"></i></a>
<a class="btn_twitter" rel="nofollow" href="javascript:;" id="twitter" data-url="http://kinhdoanh.vnexpress.net/tin-tuc/quoc-te/phu-thuy-trang-diem-goc-viet-va-cach-mang-trong-nganh-my-pham-the-gioi-3341639.html" title="Chia sẻ bài viết lên twitter"><i class="ic ic-twitter"></i></a>
