	//var PAGE_FOLDER = 1;
	
	function tabProduct(nameTab) {		 
        $('.' + nameTab + ' a').click(function() {
            var $this = $(this);
            var id = $this.attr("rel");
            $('.' + nameTab).parent().removeClass('mtd-act');
            $('.' + nameTab).parent().addClass('mtd-nor');
            $('.' + id).children().removeClass('mtd-nor');
            $('.' + id).children().addClass('mtd-act');
            $('.tbl').css('display', 'none');
            $('#' + id).css('display', 'block');      
			jQuery('.scroll_box').jScrollPane({ showArrows: true });// scroll rate table	      
        });
					
    }
	$(function() {
		//tabProduct('mmid');   
		jQuery('.scroll_box').jScrollPane({ showArrows: true });// scroll rate table			
	});
</script>


