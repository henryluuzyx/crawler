                // You can get and set dates with moment objects
                var current_datetime = new Date();
                var date_picker_hidden_input = document.getElementById('datepicker_value');
                var data_picker_trigger = document.getElementById('datepicker_cate');
                var picker = new Pikaday(
                {
                    field: date_picker_hidden_input,
                    trigger: data_picker_trigger,
                    firstDay: 1,
                    i18n: {
                        previousMonth : 'Tháng trước',
                        nextMonth     : 'Tháng tới',
                        months        : ['Tháng 1','Tháng 2','Tháng 3','Tháng 4','Tháng 5','Tháng 6','Tháng 7','Tháng 8','Tháng 9','Tháng 10','Tháng 11','Tháng 12'],
                        weekdays      : ['Chủ nhật','Thứ 2','Thứ 3','Thứ 4','Thứ 5','Thứ 6','Thứ 7'],
                        weekdaysShort : ['CN','T2','T3','T4','T5','T6','T7']
                    },
                    minDate: new Date('2003-01-01'),
                    maxDate: current_datetime,
                    defaultDate: current_datetime,
                    showMonthAfterYear: true,
                    yearRange: [2003, current_datetime.getFullYear()],
                    onSelect: function(newDate) {
                        var sendDate = newDate.getDate() + '-' + (newDate.getMonth() + 1) + '-' + newDate.getFullYear();
                        date_picker_hidden_input.value = sendDate;
                        window.location.replace('http://tuoitre.vn/tim-kiem/ban-doc/duong-day-nong/?to=' + sendDate);
                    },

                });
            

				var selected = 0;
				if(selected!=0){
					$('#thitruong-'+selected).addClass('active').css('display','block');
				}else{
					$('.block-qc-1 div').first().addClass('active').css('display','block');
				}

				var thitruongtab_active = $('.thitruongtab.active');
				if(thitruongtab_active.length >1){
					for(var i=0;i<thitruongtab_active.length;i++){
						if($(thitruongtab_active[i]).attr('id') != 'thitruong-'+selected){
							$(thitruongtab_active[i]).removeClass('active').css('display','none');
						}
					}
				}

				$('.block-qc-1 select').change(function(){
					var div = $(this).val();
					$('.block-qc-1 div.active').removeClass('active').hide();
					$('#'+div).addClass('active').show();
				});
			
