        <script type="text/javascript">
    $('.fb_share').click(function(){
        var data_url = "http://vnexpress.net/su-kien/top-rich-2015/top100";
        var linkFB = "https://www.facebook.com/sharer/sharer.php?u=" + data_url;
        window.open(linkFB,'_blank','height=450,width=700,resizable=no,status=no');
        return false;
    });
    $('.tw_share').click(function(){
        var data_url = "http://vnexpress.net/su-kien/top-rich-2015/top100";
        var linkTw = "https://twitter.com/intent/tweet?source=webclient&text=Top 100 người giàu nhất trên sàn chứng khoán 2015 - VnExpress http://vnexpress.net/su-kien/top-rich-2015/top100";
        window.open(linkTw,'_blank','height=450,width=700,resizable=no,status=no');
        return false;
    });
    $('.g_share').click(function(){
        var data_url = "http://vnexpress.net/su-kien/top-rich-2015/top100";
        var linkTw = "https://plus.google.com/share?url=" + data_url;
        window.open(linkTw,'_blank','height=450,width=700,resizable=no,status=no');
        return false;
    });
    $(document).ready(function(){
        $('#sort_bottom td a, #sort_top .sort_top_child a').each(function(key, val){
            var col = $(this).parent().attr('data-sort');
            if(col == 'education' || col == 'birthday')
            {
                $(this).append('<br /><span class="sxep '+col+'"></span>');
            }
            else
            {
                $(this).append('<span class="sxep '+col+'"></span>');
            }
        });
        var sort =  '.level_present';
                var sort_value =    'sortdow';
                $(sort).addClass(sort_value);
    });
    </script>
