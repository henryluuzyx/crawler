  $(document).ready(function(){
  	if ($.browser.chrome) {
      $('.bds-iframe').each(function() {
        $(this).attr('src',$(this).attr('src')+'#reloaded');
      });
    }
  });
  
  var BDSAPI = {
   	init: function(target, type, pos, order, token) {
      	var url = "http://api.tuoitre.vn/bds/list?type=1";
      	var date = new Date();
    	if(typeof token == 'undefined') { 
        	token = "";
        } else {
         	token = "&token=" + token; 
        }
      	url += "&pos=" + pos + "&order=" + order + token + "&time=" + date.getTime();
      	this.getData(url, function(status, data) {
          if (status == 1) {
            if (type == 1) {
                var wrapper = $("#"+target);
                wrapper.find("ul.list-bds").empty();
                for(var k in data.Items) {
                    var item = data.Items[k];
                    wrapper.find("ul.list-bds").append("<li><a target=\"_blank\" href="+item.LinkUrl+">"+item.Title+"</a></li>");
                    wrapper.show();
                }
            } else if (type == 2) {
				var wrapper = $("#"+target);
                wrapper.find("ul.list-bds-1").empty();
                for(var k in data.Items) {
                    var item = data.Items[k];
                  	var itemHtml = "<a target=\"_blank\" href="+item.LinkUrl+"><img src="+item.Image+"></a>";
                    itemHtml += "<h3><a target=\"_blank\" href="+item.LinkUrl+" >"+item.Title+"</a></h3>";
                    wrapper.find("ul.list-bds-1").append("<li>"+itemHtml+"</li>");
                    wrapper.show();
                }
            } else if (type == 3) {
            	var wrapper = $("#"+target);
                wrapper.find("ul.tab-ul").empty();
                wrapper.find("div.tab").remove();
                for(var k in data.Tabs) {
                    var tab = data.Tabs[k];
                    var tabHtml = "<li><a href='#tab-"+data.Id+"-"+k+"'>"+tab.Title+"</a></li>";
                    wrapper.find("ul.tab-ul").append(tabHtml);
                    //if (k == (data.Tabs.length -1)) {
                    //    wrapper.find("ul.tab-ul").append("<a class=\"link-more\" href=\"#\">Xem tất cả</a>");
                    //}
                    var tabContent = "<div id='tab-"+data.Id+"-"+k+"' class='tab'>";
                  	tabContent += "<a target=\"_blank\" class=\"link-more\" style=\"position:absolute; right: 10px; top: -20px;\" href=\""+tab.LinkUrl+"\">Xem tất cả</a>";
                    tabContent += "<div class='carousel-5'><ul class='slides'>";
                    for(var i in tab.Items) {
                        var item = tab.Items[i];
                        var itemHtml = "<li>";
                        itemHtml += "<a target=\"_blank\" href="+item.LinkUrl+"><img src="+item.Image+" /></a>";
                        itemHtml += "<h3><a target=\"_blank\" href="+item.LinkUrl+">"+item.Title+"</a></h3>";
                        itemHtml += "</li>";
                        tabContent += itemHtml;
                    }
                    tabContent += "</ul></div></div>";
                    wrapper.append(tabContent);
                    tabBlock(wrapper.attr("id"));

                    if($(".carousel-5").length > 0){
                         $('.carousel-5').flexslider({
                            animation: "slide",
                            controlNav: false,
                            animationLoop: false,
                            slideshow: false,
                            itemWidth: 214,
                            itemMargin: 5
                         });
                         $(".carousel-5" ).animate({opacity: 1}, 1000);
                    }
                } 
                wrapper.show();  
            } else if (type == 4) {
			 	var wrapper = $("#"+target);
			  	wrapper.find("div.block-1").empty();
				var tempFirstHtml = '';
				var tempUlHtml = '';
				for(var k in data.Items) {
					var item = data.Items[k];
					if (k == 0) {
				  		tempFirstHtml += "<a class='img_160_90_1' target=\"_blank\" href="+item.LinkUrl+"><img src="+item.Image+"></a>";
				  		tempFirstHtml += "<h4><a target=\"_blank\" href="+item.LinkUrl+" >"+item.Title+"</a></h4>";
					} else {
					  	tempUlHtml += "<li><a target=\"_blank\" href="+item.LinkUrl+" >"+item.Title+"</a></li>";
					}
				} 
				wrapper.find("div.block-1").append(tempFirstHtml + "<ul class='list-1 list-border'>"+tempUlHtml+"</ul>");
				wrapper.show();
			}
          }
        });
    },
    getData: function(api, callback) {
      $.ajax({
        url: api,
        type: "GET",
        dataType: "json",
        success: function(e) {
            callback(1, e);
        },
        error: function(e) {
        	callback(0, e);
        }
      });
    }
  };
