<script type="text/javascript" src="http://static.new.tuoitre.vn/assets/tto/6c5bba9dv783/js/jquery.js"></script>
<script type="text/javascript" src="http://static.new.tuoitre.vn/assets/tto/6c5bba9dv783/js/lib.js"></script>
<script type="text/javascript" src="http://static.new.tuoitre.vn/assets/tto/6c5bba9dv783/js/classie.js"></script>
<script type="text/javascript" src="http://static.new.tuoitre.vn/assets/tto/6c5bba9dv783/js/jquery.mCustomScrollbar.concat.min.js"></script>
<script type="text/javascript" src="http://static.new.tuoitre.vn/assets/tto/6c5bba9dv783/js/start.js"></script>
<script type="text/javascript" src="http://static.new.tuoitre.vn/assets/tto/6c5bba9dv783/js/start.min.js"></script>
<script type="text/javascript" src="http://static.new.tuoitre.vn/assets/tto/6c5bba9dv783/js/comment.min.js"></script>
<script type="text/javascript" src="http://static.new.tuoitre.vn/assets/tto/6c5bba9dv783/js/plugin.js"></script>
<script type="text/javascript">
/*<![CDATA[*/
jQuery(function($) {

$('#btn-info').click(function() {
    var comment_timeout = getCookie('comment-timeout-' + $('#cm-oid').val());
    var now = getDateTime();

    // Check comment timeout
    if (comment_timeout == '') {

        var email = $('#cm-email').val();
        var name = $('#cm-name').val();

        // Validate input
        if (email != '' && name != '' && IsEmail(email)) {
            document.domain = 'tuoitre.vn';

            var current_url = window.location;

            var now = getDateTime();
            setCookie('comment-timeout-1270232', now, 30);

            var submit_link = $('#frm-comment').attr('action');

            if ($('#cm-content').val() == '') {
                $('#cm-content').val($('.txt-content').val());
            }

            $.ajax({
                dataType: 'jsonp',
                url: submit_link,
                crossDomain: true,
                data: {
                    cm_oid: $('#cm-oid').val(),
                    cm_aid: $('#cm-aid').val(),
                    cm_otitle: $('#cm-otitle').val(),
                    cm_email: email,
                    cm_name: name,
                    cm_content: $('#cm-content').val(),
                    cm_aid_tracker: 1,//TTO : id = 6 <= comment system : 1 <= data tracker system
                    cm_host: window.location.hostname,
                    cm_cid: $('#cm-cid').val(),
                    cm_user_agent: navigator.userAgent,
                    parent_id: $('#cm-pid').val()
                },
                success: function(response) {
                    
                },
            });

            $('#cm-email').val('');
            $('#cm-name').val('');
            $('.txt-content').val('');
            $('#cm-content').val('');
            closePopup('infoForm');
            openInformPopup('successForm', 1500);

            $('#comment_frame').contents().find('.wra-comment').removeClass('active');
            $('#comment_frame').contents().find('.wra-comment textarea').val('');

        } else {
            if (email == '') {
                $('.email-warning1').show();
            } else {
                $('.email-warning1').hide();
                if (IsEmail(email)) {
                    $('.email-warning2').hide();
                } else {
                    $('.email-warning2').show();
                }
            }

            if (name == '') {
                $('.name-warning').show();
            } else {
                $('.name-warning').hide();
            }
        }
    } else {
        var remain_time = parseInt(getDiffDate(comment_timeout, now, 'seconds'));
        remain_time = 30 - remain_time;
        $('#errorForm .timeout').html(remain_time);
        closePopup('infoForm');
        openPopup('errorForm');
    }
});

});
/*]]>*/
</script>
</body>
</html>
