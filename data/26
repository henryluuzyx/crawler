var arr_baomoiprxahoinoibat = [
];;
if (arr_baomoiprxahoinoibat.length > 0) {
var slots_baomoiprxahoinoibat = new Array;
var prefix = "pr_baomoiprxahoinoibat_";
for (var i = 1; i <= 10; i++) {
var id = prefix + i;
var obj = document.getElementById(id);
if (obj == null) break;
else slots_baomoiprxahoinoibat.push(obj);
}
var total = 100.0;
var dist_baomoiprxahoinoibat = [];
for (var index = 0; index < slots_baomoiprxahoinoibat.length; index++) {
var t = Math.floor(Math.random() * total);
var n = 0;
for (var i = 0; i < arr_baomoiprxahoinoibat.length; i++) {
n += arr_baomoiprxahoinoibat[i].share;
if (n >= t) {
total -= arr_baomoiprxahoinoibat[i].share;
dist_baomoiprxahoinoibat.push(arr_baomoiprxahoinoibat[i].value);
arr_baomoiprxahoinoibat.splice(i, 1);
break;
}
}
}
for (var index = 0; index < Math.min(dist_baomoiprxahoinoibat.length, slots_baomoiprxahoinoibat.length) ; index++)
slots_baomoiprxahoinoibat[index].outerHTML = dist_baomoiprxahoinoibat[index];
}
</script>
