var arr_baomoipranninhtrattunoibat = [
];;
if (arr_baomoipranninhtrattunoibat.length > 0) {
var slots_baomoipranninhtrattunoibat = new Array;
var prefix = "pr_baomoipranninhtrattunoibat_";
for (var i = 1; i <= 10; i++) {
var id = prefix + i;
var obj = document.getElementById(id);
if (obj == null) break;
else slots_baomoipranninhtrattunoibat.push(obj);
}
var total = 100.0;
var dist_baomoipranninhtrattunoibat = [];
for (var index = 0; index < slots_baomoipranninhtrattunoibat.length; index++) {
var t = Math.floor(Math.random() * total);
var n = 0;
for (var i = 0; i < arr_baomoipranninhtrattunoibat.length; i++) {
n += arr_baomoipranninhtrattunoibat[i].share;
if (n >= t) {
total -= arr_baomoipranninhtrattunoibat[i].share;
dist_baomoipranninhtrattunoibat.push(arr_baomoipranninhtrattunoibat[i].value);
arr_baomoipranninhtrattunoibat.splice(i, 1);
break;
}
}
}
for (var index = 0; index < Math.min(dist_baomoipranninhtrattunoibat.length, slots_baomoipranninhtrattunoibat.length) ; index++)
slots_baomoipranninhtrattunoibat[index].outerHTML = dist_baomoipranninhtrattunoibat[index];
}
</script>
