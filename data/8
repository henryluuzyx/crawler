for (var i = 1; i <= 10; i++) {
var id = prefix + i;
var obj = document.getElementById(id);
if (obj == null) break;
else slots_baomoiprhomenoibat.push(obj);
}
var total = 100.0;
var dist_baomoiprhomenoibat = [];
for (var index = 0; index < slots_baomoiprhomenoibat.length; index++) {
var t = Math.floor(Math.random() * total);
var n = 0;
for (var i = 0; i < arr_baomoiprhomenoibat.length; i++) {
n += arr_baomoiprhomenoibat[i].share;
if (n >= t) {
total -= arr_baomoiprhomenoibat[i].share;
dist_baomoiprhomenoibat.push(arr_baomoiprhomenoibat[i].value);
arr_baomoiprhomenoibat.splice(i, 1);
break;
}
}
}
for (var index = 0; index < Math.min(dist_baomoiprhomenoibat.length, slots_baomoiprhomenoibat.length) ; index++)
slots_baomoiprhomenoibat[index].outerHTML = dist_baomoiprhomenoibat[index];
