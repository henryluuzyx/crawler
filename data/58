<script>
var arr_baomoiprthegioinoibat = [
];;
if (arr_baomoiprthegioinoibat.length > 0) {
var slots_baomoiprthegioinoibat = new Array;
var prefix = "pr_baomoiprthegioinoibat_";
for (var i = 1; i <= 10; i++) {
var id = prefix + i;
var obj = document.getElementById(id);
if (obj == null) break;
else slots_baomoiprthegioinoibat.push(obj);
}
var total = 100.0;
var dist_baomoiprthegioinoibat = [];
for (var index = 0; index < slots_baomoiprthegioinoibat.length; index++) {
var t = Math.floor(Math.random() * total);
var n = 0;
for (var i = 0; i < arr_baomoiprthegioinoibat.length; i++) {
n += arr_baomoiprthegioinoibat[i].share;
if (n >= t) {
total -= arr_baomoiprthegioinoibat[i].share;
dist_baomoiprthegioinoibat.push(arr_baomoiprthegioinoibat[i].value);
arr_baomoiprthegioinoibat.splice(i, 1);
break;
}
}
}
for (var index = 0; index < Math.min(dist_baomoiprthegioinoibat.length, slots_baomoiprthegioinoibat.length) ; index++)
slots_baomoiprthegioinoibat[index].outerHTML = dist_baomoiprthegioinoibat[index];
}
</script>
