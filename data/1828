<script>
//xem thêm mobile
var categoryList = (function(){
    return {
        paging: function()
        {
            $("#to_be_continue a").text('Đang tải');
            var htmlarea        =   $("#to_be_continue a").attr('data-container');
            var category_id     =   $("#to_be_continue a").attr('data-category');
            var page            =   $("#to_be_continue a").attr('data-page');
            var url             =   $("#to_be_continue a").attr('data-url');
            var param = {
                category_id: category_id,
                page: page
            };
        //console.log(param);
            // post data
            $.post(base_url+url, param, function(data) {
          //console.log(data);
                if(data.error == 1)
                {
                    $('#'+htmlarea).append(data.html);
                    $("#to_be_continue a").attr('data-page', data.page);
                    $("#to_be_continue a").text('Xem tiếp');
                }
                else if(data.error == 2)
                {
                    alert(data.message);
                    $("#to_be_continue").remove();
                }
                else
                {
                    alert('Server đang bận, vui lòng thử lại vào lúc khác');
                    $("#to_be_continue").remove();
                }
            },"json");
        }
    }
})();
$(function(){
  $( "#to_be_continue a" ).bind( "click", function() {
      categoryList.paging();
    });
});
</script>
