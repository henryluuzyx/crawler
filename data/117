];;
if (arr_baomoiprgiaitrinoibat.length > 0) {
var slots_baomoiprgiaitrinoibat = new Array;
var prefix = "pr_baomoiprgiaitrinoibat_";
for (var i = 1; i <= 10; i++) {
var id = prefix + i;
var obj = document.getElementById(id);
if (obj == null) break;
else slots_baomoiprgiaitrinoibat.push(obj);
}
var total = 100.0;
var dist_baomoiprgiaitrinoibat = [];
for (var index = 0; index < slots_baomoiprgiaitrinoibat.length; index++) {
var t = Math.floor(Math.random() * total);
var n = 0;
for (var i = 0; i < arr_baomoiprgiaitrinoibat.length; i++) {
n += arr_baomoiprgiaitrinoibat[i].share;
if (n >= t) {
total -= arr_baomoiprgiaitrinoibat[i].share;
dist_baomoiprgiaitrinoibat.push(arr_baomoiprgiaitrinoibat[i].value);
arr_baomoiprgiaitrinoibat.splice(i, 1);
break;
}
}
}
for (var index = 0; index < Math.min(dist_baomoiprgiaitrinoibat.length, slots_baomoiprgiaitrinoibat.length) ; index++)
slots_baomoiprgiaitrinoibat[index].outerHTML = dist_baomoiprgiaitrinoibat[index];
}
