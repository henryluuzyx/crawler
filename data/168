];;
if (arr_baomoiprdinhduonglamdepnoibat.length > 0) {
var slots_baomoiprdinhduonglamdepnoibat = new Array;
var prefix = "pr_baomoiprdinhduonglamdepnoibat_";
for (var i = 1; i <= 10; i++) {
var id = prefix + i;
var obj = document.getElementById(id);
if (obj == null) break;
else slots_baomoiprdinhduonglamdepnoibat.push(obj);
}
var total = 100.0;
var dist_baomoiprdinhduonglamdepnoibat = [];
for (var index = 0; index < slots_baomoiprdinhduonglamdepnoibat.length; index++) {
var t = Math.floor(Math.random() * total);
var n = 0;
for (var i = 0; i < arr_baomoiprdinhduonglamdepnoibat.length; i++) {
n += arr_baomoiprdinhduonglamdepnoibat[i].share;
if (n >= t) {
total -= arr_baomoiprdinhduonglamdepnoibat[i].share;
dist_baomoiprdinhduonglamdepnoibat.push(arr_baomoiprdinhduonglamdepnoibat[i].value);
arr_baomoiprdinhduonglamdepnoibat.splice(i, 1);
break;
}
}
}
for (var index = 0; index < Math.min(dist_baomoiprdinhduonglamdepnoibat.length, slots_baomoiprdinhduonglamdepnoibat.length) ; index++)
slots_baomoiprdinhduonglamdepnoibat[index].outerHTML = dist_baomoiprdinhduonglamdepnoibat[index];
}
