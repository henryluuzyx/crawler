$(function(){
    var owlphimchieurap = $("#phimchieurap #owl-phimchieurap");
    owlphimchieurap.owlCarousel({
        autoPlay: 4000,
        items : 1,
        itemsDesktop : [1199,1],
        itemsTablet: [600,1], //2 items between 600 and 0
        itemsDesktopSmall : [900,1],// betweem 900px and 601px
        pagination:false,
        lazyLoad:false
    });
    $("#phimchieurap .next_slider").click(function(){
        owlphimchieurap.trigger('owl.next');
    });
    $("#phimchieurap .prev_slider").click(function(){
        owlphimchieurap.trigger('owl.prev');
    });
});
