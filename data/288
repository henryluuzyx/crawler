    <script type="text/javascript">
    EqualHeight.init();
    var VnExpress = {
        social: function() {
            EqualHeight.init();
            $('.fb_share, .tw_share, .gg_share').on('click', function(){
                var action = $(this).attr('data-type');
                if(typeof(action) != 'undefined' && action)
                {
                    if(action == 'fb_share')
                    {
                        var linkFB = "https://www.facebook.com/sharer/sharer.php?u=" + current_url;
                        window.open(linkFB,'_blank','height=450,width=700,resizable=no,status=no');
                    }
                    else if(action == 'tw_share')
                    {
                        var shortUrl = '';
                        $.getJSON("http://api.bit.ly/v3/shorten?login=o_1g23qvlqab&apiKey=R_0074fe3a89174f07ac13f1b47956f1eb&longUrl="+current_url, function(data) {
                            if(typeof(data.data.url) != 'undefined' && data.data.url != '')
                            {
                                shortUrl = data.data.url;
                            }
                        });
                        setTimeout(function(){
                            if(shortUrl)
                            {
                                var linkTw = "https://twitter.com/intent/tweet?source=webclient&text="+$('title').text()+" - VnExpress "+shortUrl;
                                window.open(linkTw,'_blank','height=450,width=700,resizable=no,status=no');
                            }
                        }, 1000);
                    }
                    else if(action == 'gg_share')
                    {
                        var linkGG = "https://plus.google.com/share?url="+current_url;
                        window.open(linkGG,'_blank','height=450,width=550,resizable=no,status=no');
                    }
                }
            });
        }
    }
    var categoryList = (function(){
        return {
            paging: function() {
                $(document).ready(function() {
                    var url = $("#paging").attr('data-url');
                    var category_id = $("#paging").attr('data-category');
					var isLoad = 0;
                    // Each time the user scrolls
					$(window).scroll(function() {
						if($(window).scrollTop() + $(window).height() >= $(document).height() - 85 && isLoad == 0) {
							isLoad = 1;
							var page = $("#paging").attr('data-page');
                            if ($("#paging").attr('data-error') == 2) {
                                return false;
                            }
							$('#loading').html('<div class="loading">Đang tải dữ liệu&#8230;</div>');
                            $.ajax({
                                url: url,
                                dataType: 'json',
                                data: {category_id: category_id, page: page},
                                success: function(data) {
                                    $("#paging").attr('data-error', data.error);
                                    if (data.html && data.error == 1) {
                                        $("#paging").attr('data-page', data.page);
                                        $("#paging").append(data.html);
                                        EqualHeight.init();
                                    }
									$('#loading').empty();
									isLoad = 0;
                                }
                            });
						}
					});
                });
            }
        }
    })();
    VnExpress.social();
    categoryList.paging();
    /*scroll top*/
    $(window).scroll(function() {
        if($(window).scrollTop() >= 200)
        {
            $('#to_top').fadeIn();
        }
        else
        {
            $('#to_top').fadeOut();
        }
    });

    </script>
    <script type="text/javascript">
        $(window).scroll(function() {
            if($(window).scrollTop() >= 150) {
                $('#my_taskbar_30y').addClass('top_bar_mobile');
            } else {
                $('#my_taskbar_30y').removeClass('top_bar_mobile');
            }
        });
    </script>
