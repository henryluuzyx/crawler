<script type="text/javascript">
	$(document).ready(function() {
		eCommerce.sildebanner.bannerhome();
		if (device_env == 4) {	
			$('#carousel li').on('mouseover',function(){
				$(this).trigger('click');
			});
			$('#carousel li').each(function(){
				var heightView = $(this).height();
				var heightBlock = $(this).children('.box_title_slide').height();
				var posY = (heightView - heightBlock)/2;
				$(this).children('.box_title_slide').css({"margin-top": posY})
			});
		}
		function clickBanner(url) {
			location.href = url;
		}
	});
</script>
