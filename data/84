var arr_baomoiprkinhdoanhnoibat = [
];;
if (arr_baomoiprkinhdoanhnoibat.length > 0) {
var slots_baomoiprkinhdoanhnoibat = new Array;
var prefix = "pr_baomoiprkinhdoanhnoibat_";
for (var i = 1; i <= 10; i++) {
var id = prefix + i;
var obj = document.getElementById(id);
if (obj == null) break;
else slots_baomoiprkinhdoanhnoibat.push(obj);
}
var total = 100.0;
var dist_baomoiprkinhdoanhnoibat = [];
for (var index = 0; index < slots_baomoiprkinhdoanhnoibat.length; index++) {
var t = Math.floor(Math.random() * total);
var n = 0;
for (var i = 0; i < arr_baomoiprkinhdoanhnoibat.length; i++) {
n += arr_baomoiprkinhdoanhnoibat[i].share;
if (n >= t) {
total -= arr_baomoiprkinhdoanhnoibat[i].share;
dist_baomoiprkinhdoanhnoibat.push(arr_baomoiprkinhdoanhnoibat[i].value);
arr_baomoiprkinhdoanhnoibat.splice(i, 1);
break;
}
}
}
for (var index = 0; index < Math.min(dist_baomoiprkinhdoanhnoibat.length, slots_baomoiprkinhdoanhnoibat.length) ; index++)
slots_baomoiprkinhdoanhnoibat[index].outerHTML = dist_baomoiprkinhdoanhnoibat[index];
}
