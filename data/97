<script>
var arr_baomoiprgiaoducnoibat = [
];;
if (arr_baomoiprgiaoducnoibat.length > 0) {
var slots_baomoiprgiaoducnoibat = new Array;
var prefix = "pr_baomoiprgiaoducnoibat_";
for (var i = 1; i <= 10; i++) {
var id = prefix + i;
var obj = document.getElementById(id);
if (obj == null) break;
else slots_baomoiprgiaoducnoibat.push(obj);
}
var total = 100.0;
var dist_baomoiprgiaoducnoibat = [];
for (var index = 0; index < slots_baomoiprgiaoducnoibat.length; index++) {
var t = Math.floor(Math.random() * total);
var n = 0;
for (var i = 0; i < arr_baomoiprgiaoducnoibat.length; i++) {
n += arr_baomoiprgiaoducnoibat[i].share;
if (n >= t) {
total -= arr_baomoiprgiaoducnoibat[i].share;
dist_baomoiprgiaoducnoibat.push(arr_baomoiprgiaoducnoibat[i].value);
arr_baomoiprgiaoducnoibat.splice(i, 1);
break;
}
}
}
for (var index = 0; index < Math.min(dist_baomoiprgiaoducnoibat.length, slots_baomoiprgiaoducnoibat.length) ; index++)
slots_baomoiprgiaoducnoibat[index].outerHTML = dist_baomoiprgiaoducnoibat[index];
}
</script>
