    	$('#menu_web').find('.item_have_more_menu').click(
    		function(){
    			if($(this).find('.sub_main_menu_item').css('display') == "none"){
    				$(this).find('.sub_main_menu_item').show();
    				$(this).addClass('active');
    			}
