<script>
var arr_baomoiprnghethuatnoibat = [
];;
if (arr_baomoiprnghethuatnoibat.length > 0) {
var slots_baomoiprnghethuatnoibat = new Array;
var prefix = "pr_baomoiprnghethuatnoibat_";
for (var i = 1; i <= 10; i++) {
var id = prefix + i;
var obj = document.getElementById(id);
if (obj == null) break;
else slots_baomoiprnghethuatnoibat.push(obj);
}
var total = 100.0;
var dist_baomoiprnghethuatnoibat = [];
for (var index = 0; index < slots_baomoiprnghethuatnoibat.length; index++) {
var t = Math.floor(Math.random() * total);
var n = 0;
for (var i = 0; i < arr_baomoiprnghethuatnoibat.length; i++) {
n += arr_baomoiprnghethuatnoibat[i].share;
if (n >= t) {
total -= arr_baomoiprnghethuatnoibat[i].share;
dist_baomoiprnghethuatnoibat.push(arr_baomoiprnghethuatnoibat[i].value);
arr_baomoiprnghethuatnoibat.splice(i, 1);
break;
}
}
}
for (var index = 0; index < Math.min(dist_baomoiprnghethuatnoibat.length, slots_baomoiprnghethuatnoibat.length) ; index++)
slots_baomoiprnghethuatnoibat[index].outerHTML = dist_baomoiprnghethuatnoibat[index];
}
</script>
