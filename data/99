var slots_baomoiprhocbongduhocnoibat = new Array;
var prefix = "pr_baomoiprhocbongduhocnoibat_";
for (var i = 1; i <= 10; i++) {
var id = prefix + i;
var obj = document.getElementById(id);
if (obj == null) break;
else slots_baomoiprhocbongduhocnoibat.push(obj);
}
var total = 100.0;
var dist_baomoiprhocbongduhocnoibat = [];
for (var index = 0; index < slots_baomoiprhocbongduhocnoibat.length; index++) {
var t = Math.floor(Math.random() * total);
var n = 0;
for (var i = 0; i < arr_baomoiprhocbongduhocnoibat.length; i++) {
n += arr_baomoiprhocbongduhocnoibat[i].share;
if (n >= t) {
total -= arr_baomoiprhocbongduhocnoibat[i].share;
dist_baomoiprhocbongduhocnoibat.push(arr_baomoiprhocbongduhocnoibat[i].value);
arr_baomoiprhocbongduhocnoibat.splice(i, 1);
break;
}
}
}
for (var index = 0; index < Math.min(dist_baomoiprhocbongduhocnoibat.length, slots_baomoiprhocbongduhocnoibat.length) ; index++)
slots_baomoiprhocbongduhocnoibat[index].outerHTML = dist_baomoiprhocbongduhocnoibat[index];
}
