<script>
var arr_baomoiprdulichnoibat = [
];;
if (arr_baomoiprdulichnoibat.length > 0) {
var slots_baomoiprdulichnoibat = new Array;
var prefix = "pr_baomoiprdulichnoibat_";
for (var i = 1; i <= 10; i++) {
var id = prefix + i;
var obj = document.getElementById(id);
if (obj == null) break;
else slots_baomoiprdulichnoibat.push(obj);
}
var total = 100.0;
var dist_baomoiprdulichnoibat = [];
for (var index = 0; index < slots_baomoiprdulichnoibat.length; index++) {
var t = Math.floor(Math.random() * total);
var n = 0;
for (var i = 0; i < arr_baomoiprdulichnoibat.length; i++) {
n += arr_baomoiprdulichnoibat[i].share;
if (n >= t) {
total -= arr_baomoiprdulichnoibat[i].share;
dist_baomoiprdulichnoibat.push(arr_baomoiprdulichnoibat[i].value);
arr_baomoiprdulichnoibat.splice(i, 1);
break;
}
}
}
for (var index = 0; index < Math.min(dist_baomoiprdulichnoibat.length, slots_baomoiprdulichnoibat.length) ; index++)
slots_baomoiprdulichnoibat[index].outerHTML = dist_baomoiprdulichnoibat[index];
}
</script>
