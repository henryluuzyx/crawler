];;
if (arr_baomoiprtinhyeuhonnhannoibat.length > 0) {
var slots_baomoiprtinhyeuhonnhannoibat = new Array;
var prefix = "pr_baomoiprtinhyeuhonnhannoibat_";
for (var i = 1; i <= 10; i++) {
var id = prefix + i;
var obj = document.getElementById(id);
if (obj == null) break;
else slots_baomoiprtinhyeuhonnhannoibat.push(obj);
}
var total = 100.0;
var dist_baomoiprtinhyeuhonnhannoibat = [];
for (var index = 0; index < slots_baomoiprtinhyeuhonnhannoibat.length; index++) {
var t = Math.floor(Math.random() * total);
var n = 0;
for (var i = 0; i < arr_baomoiprtinhyeuhonnhannoibat.length; i++) {
n += arr_baomoiprtinhyeuhonnhannoibat[i].share;
if (n >= t) {
total -= arr_baomoiprtinhyeuhonnhannoibat[i].share;
dist_baomoiprtinhyeuhonnhannoibat.push(arr_baomoiprtinhyeuhonnhannoibat[i].value);
arr_baomoiprtinhyeuhonnhannoibat.splice(i, 1);
break;
}
}
}
for (var index = 0; index < Math.min(dist_baomoiprtinhyeuhonnhannoibat.length, slots_baomoiprtinhyeuhonnhannoibat.length) ; index++)
slots_baomoiprtinhyeuhonnhannoibat[index].outerHTML = dist_baomoiprtinhyeuhonnhannoibat[index];
}
