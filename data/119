</section>
<script>
var arr_baomoipramnhacnoibat = [
];;
if (arr_baomoipramnhacnoibat.length > 0) {
var slots_baomoipramnhacnoibat = new Array;
var prefix = "pr_baomoipramnhacnoibat_";
for (var i = 1; i <= 10; i++) {
var id = prefix + i;
var obj = document.getElementById(id);
if (obj == null) break;
else slots_baomoipramnhacnoibat.push(obj);
}
var total = 100.0;
var dist_baomoipramnhacnoibat = [];
for (var index = 0; index < slots_baomoipramnhacnoibat.length; index++) {
var t = Math.floor(Math.random() * total);
var n = 0;
for (var i = 0; i < arr_baomoipramnhacnoibat.length; i++) {
n += arr_baomoipramnhacnoibat[i].share;
if (n >= t) {
total -= arr_baomoipramnhacnoibat[i].share;
dist_baomoipramnhacnoibat.push(arr_baomoipramnhacnoibat[i].value);
arr_baomoipramnhacnoibat.splice(i, 1);
break;
}
}
}
for (var index = 0; index < Math.min(dist_baomoipramnhacnoibat.length, slots_baomoipramnhacnoibat.length) ; index++)
slots_baomoipramnhacnoibat[index].outerHTML = dist_baomoipramnhacnoibat[index];
}
</script>
