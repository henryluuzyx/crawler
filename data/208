<script>
var arr_baomoiprquanlyquyhoachnoibat = [
];;
if (arr_baomoiprquanlyquyhoachnoibat.length > 0) {
var slots_baomoiprquanlyquyhoachnoibat = new Array;
var prefix = "pr_baomoiprquanlyquyhoachnoibat_";
for (var i = 1; i <= 10; i++) {
var id = prefix + i;
var obj = document.getElementById(id);
if (obj == null) break;
else slots_baomoiprquanlyquyhoachnoibat.push(obj);
}
var total = 100.0;
var dist_baomoiprquanlyquyhoachnoibat = [];
for (var index = 0; index < slots_baomoiprquanlyquyhoachnoibat.length; index++) {
var t = Math.floor(Math.random() * total);
var n = 0;
for (var i = 0; i < arr_baomoiprquanlyquyhoachnoibat.length; i++) {
n += arr_baomoiprquanlyquyhoachnoibat[i].share;
if (n >= t) {
total -= arr_baomoiprquanlyquyhoachnoibat[i].share;
dist_baomoiprquanlyquyhoachnoibat.push(arr_baomoiprquanlyquyhoachnoibat[i].value);
arr_baomoiprquanlyquyhoachnoibat.splice(i, 1);
break;
}
}
}
for (var index = 0; index < Math.min(dist_baomoiprquanlyquyhoachnoibat.length, slots_baomoiprquanlyquyhoachnoibat.length) ; index++)
slots_baomoiprquanlyquyhoachnoibat[index].outerHTML = dist_baomoiprquanlyquyhoachnoibat[index];
}
</script>
