var arr_baomoiprsuckhoeytenoibat = [
];;
if (arr_baomoiprsuckhoeytenoibat.length > 0) {
var slots_baomoiprsuckhoeytenoibat = new Array;
var prefix = "pr_baomoiprsuckhoeytenoibat_";
for (var i = 1; i <= 10; i++) {
var id = prefix + i;
var obj = document.getElementById(id);
if (obj == null) break;
else slots_baomoiprsuckhoeytenoibat.push(obj);
}
var total = 100.0;
var dist_baomoiprsuckhoeytenoibat = [];
for (var index = 0; index < slots_baomoiprsuckhoeytenoibat.length; index++) {
var t = Math.floor(Math.random() * total);
var n = 0;
for (var i = 0; i < arr_baomoiprsuckhoeytenoibat.length; i++) {
n += arr_baomoiprsuckhoeytenoibat[i].share;
if (n >= t) {
total -= arr_baomoiprsuckhoeytenoibat[i].share;
dist_baomoiprsuckhoeytenoibat.push(arr_baomoiprsuckhoeytenoibat[i].value);
arr_baomoiprsuckhoeytenoibat.splice(i, 1);
break;
}
}
}
for (var index = 0; index < Math.min(dist_baomoiprsuckhoeytenoibat.length, slots_baomoiprsuckhoeytenoibat.length) ; index++)
slots_baomoiprsuckhoeytenoibat[index].outerHTML = dist_baomoiprsuckhoeytenoibat[index];
}
